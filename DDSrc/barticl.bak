     Procedure private.Transaction_Cascade_Update_Bmove
               Integer liBmove_dd

               //§˜¢˜ âª ¡˜  ¤âœª « £âª «¦¬ barticl
               String   lsB_Btypkin_Recr     lsA_Btypkin_Recr
               Date     ldB_Barticl_Hmnia    ldA_Barticl_Hmnia
               Integer  liB_Barticl_No       liA_Barticl_No

