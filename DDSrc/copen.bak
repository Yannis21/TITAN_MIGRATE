        // Copen.Copen_P00

        // Copen.Copen_Xr

        // Copen.Copen_Pr

        // Copen.Copen_X2

        // Copen.Copen_P2

        // Copen.Copen_C_Code2
        Set Field_Value_Table      Field Copen.Copen_C_Code2 To (Copen_Copen_C_Code2_VT(Self))

        // Copen.Copen_M_Code
        Set Field_Value_Table      Field Copen.Copen_M_Code To (Copen_Copen_M_Code_VT(Self))

        // Copen.Cmovesa_Recr

