      Function Validate_Save Returns Integer
               Integer iReturnVal

               Forward Get Validate_Save to iReturnVal

               If (Not(iReturnVal));
               Begin

                //---------‚¢úöÆ¶† õû£†¶¨®öÂò™ §‚¶¨ baxio record----------------------
                //sososo £ûß‡™ ß®úßú† §ò ßò†•‡ £ú ´û§ current record ????
                If (Baxio.Recnum =  0);
                Begin //ú·§ õû£†¶¨®öÈ §‚¶ record
                      //1007:‚¢úöÆ¶™ õû£†¶¨®öÂò™ §‚¶¨ ò•†¶ö®·≠¶¨ òß¶ ´¶ baxio_dd

                      If (pifromxrhsh(Self)=0) Begin
                        If (Not((Main_File(OPERATION_ORIGIN)) =  Bmove.File_Number)) Send Operation_Not_Allowed 1007
                      End
                End   //´‚¢¶™ ú¢‚öÆ‡§ õû£†¶¨®öÂò™ §‚¶¨ baxio record


                //---------‚¢úöÆ¶† editing existing baxio records---------------------
                Else;
                Begin //edit an existing record
                      //1008:´®¶ß¶ß¶Âû©û (edit) baxio_c_code2 £Ê§¶§ òßÊ bmove_dd

                      If (pifromxrhsh(Self)=0) Begin

                        If (;
                            ((psB_Baxio_C_Code2(Self))     <> Baxio.Baxio_C_Code2) and;
                            ((Main_File(OPERATION_ORIGIN)) <> Bmove.File_Number);
                            )                                                       Send Operation_Not_Allowed 1008
                        //1009:´®¶ß¶ß¶Âû©û (edit) baxio_v2 £Ê§¶§ òßÊ bmove_dd
                        If (;
                            ((pnB_Baxio_V2(Self))          <> Baxio.Baxio_V2      ) and;
                            ((Main_File(OPERATION_ORIGIN)) <> Bmove.File_Number);
                            )                                                       Send Operation_Not_Allowed 1009
                        //1010:´®¶ß¶ß¶Âû©û (edit) baxio_v0 £Ê§¶§ òßÊ bmove_dd
                        If (;
                            ((pnB_Baxio_V0(Self))          <> Baxio.Baxio_V0      ) and;
                            ((Main_File(OPERATION_ORIGIN)) <> Bmove.File_Number);
                            )                                                       Send Operation_Not_Allowed 1010
                        //1011:´®¶ß¶ß¶Âû©û (edit) baxio_v00 £Ê§¶§ òßÊ bmove_dd
                        If (;
                            ((pnB_Baxio_V00(Self))         <> Baxio.Baxio_V00     ) and;
                            ((Main_File(OPERATION_ORIGIN)) <> Bmove.File_Number);
                            )                                                       Send Operation_Not_Allowed 1011
                        //1012:´®¶ß¶ß¶Âû©û (edit) baxio_vr £Ê§¶§ òßÊ bmove_dd
                        If (;
                            ((pnB_Baxio_Vr(Self))          <> Baxio.Baxio_Vr      ) and;
                            ((Main_File(OPERATION_ORIGIN)) <> Bmove.File_Number);
                            )                                                       Send Operation_Not_Allowed 1012

                      End
                        // Egine rem 20/12/2001 giati an orisoume sta metrhta gia paradeigma
                        // oti to status SE einai exoflhmeno, tote xtypaei to validation sto
                        // kleisimo toy barticle. Alloste den vlepoume giati den mporei na
                        // anoigokleisei
//                      //1013:ú·§ ´¶ status úÂ§ò† ú•¶≠¢û£‚§¶, òßòö¶®úÁ¶§´ò† Ê¢ò ´ò ßú®ò†´‚®‡ edits òß¶ ´¶ bmove
//                      If (;
//                           ((piB_Baxio_Status(Self))      = 2                    ) and;
//                           ((Main_File(OPERATION_ORIGIN)) =  Bmove.File_Number);
//                         )                                                       Send Operation_Not_Allowed 1013

                      If (pifromxrhsh(Self)=0) Begin
                        //1014:òßòö¶®úÁú´ò† û ò¢¢òö„ parent baxiokat record
                        If (Not;
                            ((piB_Baxiokat_Code(Self)) =  Baxiokat.Baxiokat_Code);
                            )                                                       Send Operation_Not_Allowed 1014

                        //1015:òßòö¶®úÁú´ò† û ò¢¢òö„ parent year record
                        If (Not;
                            ((piB_Year_Year    (Self)) =  SysCurXr.Year_Year        );
                            )                                                       Send Operation_Not_Allowed 1015
                      End
                End   //´‚¢¶™ ú¢‚öÆ‡§ editing existing baxio records

                //¢¶ß¶Â ‚¢úöÆ¶†
                //1001:baxio_hmnial > 0
                If (Not(Baxio.Baxio_Hmnial > 0 ))   Send Operation_Not_Allowed 1001
                //1002:baxio_v2 > 0 (õ‚§ úß†´®‚ßú´ò† ‚§ò ò•†Êö®ò≠¶ §ò ‚Æú† £ûõú§†°„ ò•Âò ©ú §Ê£†©£ò úöö®ò≠„™
                If (Not(Baxio.Baxio_V2 > 0))         Send Operation_Not_Allowed 1002
                //1003:baxio_c_code2 <> ""
                If (Not(Baxio.Baxio_C_Code2 <>""))    Send Operation_Not_Allowed 1003
                //1004:baxio_v0 > 0 (õ‚§ úß†´®‚ßú´ò† ‚§ò ò•†Êö®ò≠¶ §ò ‚Æú† £ûõú§†°„ ò•Âò ©ú °Á®†¶ §Ê£†©£ò Æ®„©ú‡™
//--------------If (Not(Baxio.Baxio_V0 > 0))         Send Operation_Not_Allowed 1004
                //1005:baxio_v00> 0 (õ‚§ úß†´®‚ßú´ò† ‚§ò ò•†Êö®ò≠¶ §ò ‚Æú† £ûõú§†°„ ò•Âò ©ú õú¨´ú®úÁ¶§ §Ê£†©£ò Æ®„©ú‡™
//--------------If (Not(Baxio.Baxio_V00> 0))         Send Operation_Not_Allowed 1005
                //1006:baxio_vr > 0 (õ‚§ úß†´®‚ßú´ò† ‚§ò ò•†Êö®ò≠¶ §ò ‚Æú† £ûõú§†°„ ò•Âò ©ú §Ê£†©£ò reporting
//--------------If (Not(Baxio.Baxio_VR > 0))         Send Operation_Not_Allowed 1006
                //1100:cby = globals

                If (pifromxrhsh(Self)=0) Begin

                    If ( ;
                        (Not(Baxio.Company_Code = SyscurXr.Company_Code)) or;
                        (Not(Baxio.Branch_Code  = SyscurXr.Branch_Code )) or;
                        (Not(Baxio.Year_Year    = SyscurXr.Year_Year   ));
