      Procedure Update
                Forward Send Update
                
                If (Baxio.Baxio_C_Code2='') Move SyscurXr.C_Code0 to Baxio.Baxio_C_Code2
                
                //ò) ú§û£‚®‡©û £„ related ßúõÂ‡§
                Move Syscurxr.Company_Code  to Baxio.Company_Code
                Move Syscurxr.Branch_Code   to Baxio.Branch_Code
                Move Syscurxr.Year_Year     to Baxio.Year_Year
                Move Syscurxr.Year_Recr     to Baxio.Year_Recr
                Move Baxiokat.Baxiokat_Code to Baxio.Baxiokat_Code

                //ö) õ†òÆúÂ®†©û calculated ßúõÂ‡§
                //ö01:ú• ¶®†©£¶Á, ´ò ß¢û®‡´‚ò õ‚§ ‚Æ¶¨§ ©´¶†ÆúÂò ßò®ò¢¢òô„™
                If (Baxiokat.Baxiokat_Type = 2);
                Begin //ò•†Êö®ò≠ò ß¢û®‡´‚ò
                     Move "" to Baxio.Baxio_Code_In
                     Move "" to Baxio.Baxio_Epon_In
                     Move 0  to Baxio.Baxio_Fpar_In
                End
                //ö02:yyyy + mm + dd ¢„•û™ ö†ò ´ò reports
                Move (TheYearSegment (Baxio.Baxio_Hmnial)) to Baxio.Baxio_Lixi_YYYY
                Move (TheMonthSegment(Baxio.Baxio_Hmnial)) to Baxio.Baxio_Lixi_MM
                Move (TheDaySegment  (Baxio.Baxio_Hmnial)) to Baxio.Baxio_Lixi_DD

                //ö03:ß®È´¶™ & ´ú¢ú¨´òÂ¶™ ßò®ò¢„ß´û™
                If (Baxio.Baxio_Lcode = "");
                Begin
                     Move (-1) to Baxio.Baxio_File_Out
                     Move ""   to Baxio.Baxio_Code_Out
                     Move ""   to Baxio.Baxio_Epon_Out
                     Move ""   to Baxio.Baxio_Fpar_Out

                     Move (-1) to Baxio.Baxio_Lfile
                     Move ""   to Baxio.Baxio_Lepon
                     Move ""   to Baxio.Baxio_Lpar
                End
      End_Procedure //update


