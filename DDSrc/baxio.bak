      Procedure Constrain
                Forward Send Constrain
                If (piMustNOTConstrain(Self)=1) Begin
                End 
                else If (piMustNOTConstrain(Self)=2) Begin 
                    Constrain Baxio.Company_Code Eq Syscurxr.Company_Code
                    Constrain Baxio.Branch_Code  Eq Syscurxr.Branch_Code
                end    
                Else Begin
                    Constrain Baxio.Company_Code Eq Syscurxr.Company_Code
                    Constrain Baxio.Branch_Code  Eq Syscurxr.Branch_Code
                    Constrain Baxio.Year_Year    Eq Syscurxr.Year_Year
                End
      End_Procedure   //constrain


      //ò) ú§û£‚®‡©û £„ related ßúõÂ‡§
      //ô) ò§·ö§‡©û properties
