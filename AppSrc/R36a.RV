    Procedure Activate_View Returns Integer
        String sStr sModule StrDisp stext
        date ddate
        Integer rVal iint icheck
        string scompany
        sysdate4 ddate
        set value of (f_hme_apo(groupsub4(current_object))) to ddate
        set value of (f_hme_eos(groupsub4(current_object))) to ddate
    
        Move '' to sStr
        send rebuild_constraints to baxiokat_dd
        send beginning_of_data to dblstbaxiokat true
        Forward Get Msg_Activate_View to rVal
    
        Get psMenuModule  Of Main_Menu  to sModule
        Set psModule                    to sModule
        Send Delete_Data            to (Selected_Items(dblstBRANCH(self)))
        Send Find to (branch_dd(self)) First_Record 1
        Set Dynamic_Update_State of (dblstbranch(self)) to false
        While (Found=1)
            get field_current_value of branch_dd field branch.company_code to scompany
            get field_current_value of branch_dd field branch.branch_apoth to icheck
            if ((syscurxr.company_code=scompany) and (icheck=1));
                Set Array_Value of (Selected_Items(dblstbranch(self))) Item iInt to branch.Recnum
            Add 1 to iInt
            Send Find to (branch_dd(self)) Next_Record 1
        Loop
        Send Beginning_Of_Data    to (dblstbranch(self))
        Set Dynamic_Update_State of (dblstbranch(self)) to true
        Procedure_Return rVal
    
    End_Procedure
    
    Object axioWQLReport Is A crystalreportSubSQL
    
        Property Integer ReportReady Public False
    
        Set Report_View_id To (Parent(Current_object))
    
        Procedure OnStartPrinting
            integer iint1 icount1 irec1 icode
            date  apo eos
            Local string  Selection FileName slabel sbranch sone stext1 sbank
            Local integer Starthmen Endhmen stbaxio FileType Destination ;
                          SortOrder NumSortFields CurSortField SortDirection wdate
            Integer iint icheck icount irec
            string  stext scode sname
            move 'BANK' to sbank
            //*** Setup the selection formula
            move '1' to sone
            Move "" To Selection
            get psbranch       to sbranch
            get starthme       to starthmen
            Get Endhme         To Endhmen
    
            Append Selection (If(Selection <> "", " and ", ""))"{bmove.company_code} = " '"' (Trim(syscurxr.company_code)) '"'
            Get Select_Count of (dblstbranch(Self)) to iInt
            If (iInt>0) Begin
                move '' to stext
                For iCount from 0 to (iInt-1)
                    Get array_value of (Selected_Items(dblstbranch(self))) Item iCount to iRec
                    Clear branch
                    Move iRec to branch.Recnum
                    Find Eq branch.Recnum
                    move branch.branch_code to scode
                    move branch.branch_name to sname
                    Move (Append(sbranch,(trim(sname)+' '))) to sbranch
                    Move (Append(stext,(trim(scode)+' '))) to stext
                Loop
            End
            Append Selection (If(Selection <> "", " and ", "")) "InStr('" stext "',{bmove.branch_code})<>0"
            Append Selection (If(Selection <> "", " and ", ""))"{bmove.year_year} = " '' (syscurxr.year_year) ''
            Append Selection (If(Selection <> "", " and ", ""))"{btypkin.btypkin_anti_xp} = 2"
            Append Selection (If(Selection <> "", " and ", ""))"{btypkin.btypkin_sign} = 1"
            Append Selection (If(Selection <> "", " and ", ""))"{barticl.barticl_ekleise} = " '"' (Trim(sone)) '"'
            Append Selection (If(Selection <> "", " and ", ""))"{codemast.type} = " '"' (Trim(sbank)) '"'
    
            If (trim(starthmen)) ne 0 ;
                  Append Selection (If(Selection <> "", " and ", "")) "{bmove.barticl_hmnia} >= " (winql_date(starthmen))
    
            If (trim(Endhmen)) ne 0 ;
                  Append Selection (If(Selection <> "", " and ", "")) "{bmove.barticl_hmnia} <= " (winql_date(Endhmen))
    
            Get Select_Count of (dblstbaxiokat(Self)) to iInt1
            If (iInt1>0) Begin
            move '' to stext1
            For iCount1 from 0 to (iInt1-1)
                Get array_value of (Selected_Items(dblstbaxiokat(self))) Item iCount1 to iRec1
                Clear baxiokat
                Move iRec1 to baxiokat.Recnum
                Find Eq baxiokat.Recnum
                move baxiokat.baxiokat_code to icode
                move (Append(stext1,'#'+(String(icode)))+'#') to stext1
            Loop
           end
    
          Append Selection (If(Selection <> "", " and ", "")) "InStr('" stext1 "',('#'+TOTEXT ({baxio.baxiokat_code},0)+'#'))<>0"
            Set SelectionFormula To Selection
            If Selection Eq "" ;
                Set Formula "SelTxt" To ('"' + "NONE" + '"')
            Else ;
                Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
    
            move starthmen to apo
            move endhmen   to eos
            Set Formula "fapo"  To (winql_date(apo))
            Set Formula "feos"  To (winql_date(eos))
            set formula "fbranch" to (winql_string(sbranch))
            get value of form3 to slabel
            Set Formula "flabel" to (Winql_String(slabel))
            Get NSortFields To NumSortFields
            For CurSortField From 0 To (NumSortFields - 1)
            Send DeleteNthSortField 0
            Loop
         //   get descasc to sortdirection
            move PE_SF_asCENDING to sortdirection
     //       set GroupField PE_GROUPHEADER to "{bMOVE.barticl_hmnia}"
            Set GroupSortDirection PE_GROUPHEADER to SortDirection
            Send AddSortField "{baxio.baxio_code_in}" SortDirection
            //*** Handle export to file
            Get Output_Destination To Destination
            If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                Get WQLFileName To FileName
                Get WQLFileType To FileType
    
                Set ExportFileName   To FileName
                Set ExportFormatType To FileType
            End
            Else If Destination Eq WQL_PRINT_TO_PRINTER ;
            Send SelectAndSetPrinter
    
            //*** Setup the report if ready property
            If Destination EQ WQL_PRINT_TO_WINDOW ;
                Set ReportReady To False
            Else ;
                Set ReportReady To True
        End_procedure // OnStartPrinting
    
    
        Procedure RunaxioReport
            //*** We will be busy, change the cursor
            Send Cursor_Wait To (Cursor_Control(Current_Object))
            Set Report_name  To (psrep(self))
            //*** Run the report
            Send Run_report
    
            //*** We are ready, change the cursor
            Send Cursor_Ready To (Cursor_Control(Current_Object))
    
            //*** Report you are finsihed for those seetings that do not use the
            //*** screen
            If (ReportReady(Current_object)) ;
                Send Info_box "WinQL report ready."
        End_procedure // RunCustReport
    
    End_object // CustWQLReport
    
