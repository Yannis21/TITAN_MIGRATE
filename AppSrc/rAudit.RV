            Procedure OnStartPrinting
                String  Selection FileName sUser sStatus sComp sCompName sBranch sBranchName sStatusFilter
                Integer FileType Destination SortOrder iWhat iStatus
                Integer NumSortFields CurSortField SortDirection
                Date    dHmnia dFrom dTo
        
                Move "" To Selection
        
                Move SyscurXr.Company_Code  to sComp
                Move SyscurXr.Company_Name  to sCompName
                Move SyscurXr.Branch_Code   to sBranch
                Move SyscurXr.Branch_Name   to sBranchName
        
                Get fdFrom      to dFrom
                Get fdTo        to dTo
                Get fsBranch    to sBranch
                Get fsUser      to sUser
                Get piWhat      to iWhat
                Get piStatus    to iStatus
        
                     If (iStatus=0) Move 'Ó¢ò'              to sStatus
                Else If (iStatus=1) Move 'É†¶®üÈ©ú†™'       to sStatus
                Else If (iStatus=2) Move 'å‚ú™ úöö®ò≠‚™'    to sStatus
                Else If (iStatus=3) Move 'É†òö®ò≠‚™'        to sStatus
        
                     If (iStatus=1) Move 'E'    to sStatusFilter
                Else If (iStatus=2) Move 'N'    to sStatusFilter
                Else If (iStatus=3) Move 'D'    to sStatusFilter
        
                If (dFrom  <>'') Append Selection (If(Selection <> "", " and ", ""))"{Gmove.Garticl_Hmnia} >= " (Winql_Date(dFrom))
                If (dTo    <>'') Append Selection (If(Selection <> "", " and ", ""))"{Gmove.Garticl_Hmnia} <= " (Winql_Date(dTo))
                If (sBranch<>'') Append Selection (If(Selection <> "", " and ", ""))"{Gmove.Branch_Code}= " '"' (Trim(sBranch)) '"'
                If (sUser  <>'') Append Selection (If(Selection <> "", " and ", ""))"{Gmove.Login_ID}   = " '"' (Trim(sUser)) '"'
        
                                 Append Selection (If(Selection <> "", " and ", ""))"{Gmove.Gtypkin_Code}      = " (Winql_Number(iWhat))
                If (iStatus<>0)  Append Selection (If(Selection <> "", " and ", ""))"{Gmove.Eidos_metabolhs}   = " '"' (Trim(sStatusFilter)) '"'
        
        
                Set SelectionFormula To Selection
        
                If (sUser<>'')  Set Formula "fuserid"   To (winql_String(sUser))
                If (dFrom<>'')  Set Formula "fapo"      To (winql_date  (dFrom))
                If (dTo  <>'')  Set Formula "feos"      To (winql_date  (dTo))
        
                Set Formula "fcompany" To (winql_string  (sCompName))
                Set Formula "fbranch"  To (winql_string  (sBranchName))
                Set Formula "feos"     To (winql_date  (dTo))
                Set Formula "feos"     To (winql_date  (dTo))
                Set Formula "fstatus"  To (winql_string  (sStatus))
        
        
                //*** Handle export to file
                Get Output_Destination To Destination
        
                If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                    Get WQLFileName To FileName
                    Get WQLFileType To FileType
        
                    Set ExportFileName   To FileName
                    Set ExportFormatType To FileType
                End
                If Destination EQ WQL_PRINT_TO_WINDOW ;
                    Set ReportReady To False
                Else ;
                    Set ReportReady To True
            End_procedure // OnStartPrinting
        
        
            Procedure RunHmerProgReport
               Integer iWhat
               String  sWhat sRepName
               Move ""              to sWhat
               Get piWhat           to iWhat
               Move (String(iWhat)) to sWhat
               If (Length(sWhat)=1) Insert '0' in sWhat at 1
               Move ("r"+Trim(sWhat)+".rpt") to sRepName
               Set Report_name  to sRepname
               Send Run_report
               Send Cursor_Ready To (Cursor_Control(Current_Object))
               If (ReportReady(Current_object)) ;
                  Send Info_box "WinQL report ready."
            End_procedure // RunCustReport
        
        End_object // CustWQLReport
        
