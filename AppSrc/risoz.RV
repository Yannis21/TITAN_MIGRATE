    Object custWQLReport Is A CrystalReportSubSQL
       Set Report_View_id  to (Parent(Self))
    
    // Set Output_Destination          to WQL_PRINT_TO_WINDOW
       Set Output_Destination          to WQL_PRINT_TO_FILE
       Set ExportFileName              to "c:\isoz.txt"
       Set UseReportNumberFormat_State to True
       Set ExportFormatType            to WQL_CommaSeparatedType
       Set FieldDelimiter              to ","
       Set StringDelimiter             to '"'
       Set UseReportDateFormat_State   To True
    
        Property Integer ReportReady Public False
    
    
        Procedure OnStartPrinting
            date datef datet  apo eos
            local string  lscatypes Startantlian Endantlian sstr flysh1 flysh2
            Local integer Starthmen Endhmen FileType Destination SortOrder sepil
            Local String  Selection module FileName StartantliaNumber EndantliaNumber
            Local Integer NumSortFields CurSortField SortDirection wdate iok iwhat
            local string sbranch slabel
            local integer ianal
            number norio
            date dapo
            get value of (frmorio(groupsub4(self))) to norio
            get psbranch to sbranch
            //*** Setup the selection formula
            Move "" To Selection
            reread syscurgl
            unlock
            move syscurgl.year_xrhshfrom to dapo
            Append Selection (If(Selection <> "", " and ", ""))"{gmove.company_code} = " '"' (Trim(syscurgl.company_code)) '"'
            if (sbranch<>'') Append Selection (If(Selection <> "", " and ", ""))"{gmove.branch_code} = " '"' (Trim(sbranch)) '"'
            Append Selection (If(Selection <> "", " and ", "")) "{gmove.year_year} = " '' (syscurgl.year_year) ''
    
            Get StartNumber To StartantliaNumber
            Get EndNumber   To EndantliaNumber
            Get Endhme   To Endhmen
            If (trim(StartantliaNumber)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{gmove.gplan_code} >= " '"' (trim(startantlianumber)) '"'
    
            If (trim(EndantliaNumber)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{gmove.gplan_code} <= " '"' (trim(Endantlianumber)) '"'
    
      //      If dapo ne '' ;
     //          Append Selection (If(Selection <> "", " and ", "")) "{gmove.garticl_hmnia} >= " '' (winql_date(dapo))''
    
    
            If Endhmen ne 0 ;
               Append Selection (If(Selection <> "", " and ", "")) "{gmove.garticl_hmnia} <= " '' (winql_date(Endhmen))''
    
            get starthme to apo
            get endhme   to eos
            string sformat
            get psformat to sformat
            Set Formula "forio" to (winql_number(norio))
            Set Formula "fformat" to (winql_string(sformat))
            Set Formula "fapo" To (winql_date(apo))
            Set Formula "feos" To (winql_date(eos))
            clear branch
            move syscurgl.company_code to branch.company_code
            move sbranch               to branch.branch_code
            find eq branch by index.1
            Set Formula "fbranch" to (Winql_String(branch.branch_name))
            get value of form3 to slabel
            Set Formula "flabel" to (Winql_String(slabel))
            get analstat to ianal
            Set Formula "fanal" to (Winql_String(ianal))
            Set SelectionFormula To Selection
            If Selection Eq "" ;
                Set Formula "SelTxt" To ('"' + "NONE" + '"')
            Else ;
                Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
    
    
          //*** Setup new sort order
            move 1 To SortDirection
      End_procedure // OnStartPrinting
    
      Procedure RuncustReport
            Send Cursor_Wait To (Cursor_Control(Current_Object))
            Send Run_report
            Send Cursor_Ready To (Cursor_Control(Current_Object))
            If (ReportReady(Current_object)) ;
                Send Info_box "WinQL report ready."
        End_procedure // RunCustReport
    
    End_object // CustWQLReport
    
    
