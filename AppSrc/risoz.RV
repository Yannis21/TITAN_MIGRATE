        //AB-StoreStart
        Procedure OnClick
           string dateapo dateeos datehh datemm dateyy msgstr sapo seos slog sper
           integer listatus ideikths i1
           Number n1 n2 n3 n4 n5 n6 n7 n8 n9 n10
           Get Starthme To dateapo
           Get Endhme   To dateeos
        
           if dateeos eq "" begin
             move 31 to datehh
             move 12 to datemm
             move syscurgl.year_year to dateyy
             Append dateeos datehh
             append dateeos datemm
             append dateeos dateyy
            end
        
            if dateapo eq "" begin
              move 01 to datehh
              move 01 to datemm
              move syscurgl.year_year to dateyy
              append dateapo datehh
              append dateapo "/"
              append dateapo datemm
              append dateapo "/"
              append dateapo dateyy
        
           end
        
           move 0 to listatus
                 if (dateapo ne "") begin
                   if ((dateapo < syscurgl.year_xrhshfrom) or (dateapo > syscurgl.year_xrhshto)) begin
                      move "" to msgstr
                      move 1 to listatus
                      move "†£/¤åœª „§ ¢¦šãª „¡«æª Ž¨åà¤ •¨ã©žª" to msgstr
                      error 999 msgstr
        
                   end
                 end
        
                  if (dateeos ne "") begin
                   if ((dateeos < syscurgl.year_xrhshfrom) or (dateeos > syscurgl.year_xrhshto)) begin
                      move "" to msgstr
                       move 1 to listatus
                      move "†£/¤åœª „§ ¢¦šãª „¡«æª Ž¨åà¤ •¨ã©žª" to msgstr
                      error 999 msgstr
                   end
                  end
           if (listatus = 0) begin
                 set label of fshow to ''
                 Zerofile rp1
                 Erasefile "c:\isoz.txt"
        
                 clear gplanl
                 get value of (f_code_apo(groupsub1(self))) to sapo
                 get value of (f_code_eos(groupsub1(self))) to seos
                 if (seos='') move '999999999999999' to seos
                 move syscurgl.company_code to gplanl.company_code
                 move syscurgl.branch_code  to gplanl.branch_code
                 move sapo                  to gplanl.gplan_code
                 find ge gplanl by index.1
                 while ((found) and  (gplanl.company_code=syscurgl.company_code) and;
                       (gplanl.branch_code=syscurgl.branch_code)   and;
                       (gplanl.gplan_code ge sapo) and (gplanl.gplan_code le seos))
                    clear rp1
                    move gplanl.gplan_code to rp1.kodikos
                    move gplanl.GPLAN_DESCR1 to rp1.perigrafh
                    saverecord rp1
                    set label of fshow to gplanl.gplan_code
                    find gt gplanl by index.1
                 Loop
                 Set Report_name  of (custWQLReport(current_object)) to "iso31.rpt"
                 Send RuncustReport To (custWQLReport(current_object))
                 Send diabase_kiniseis
