    Object ButtonSub1 is a Button

        //AB-StoreTopStart
        
        //AB-StoreTopEnd

        Set Bitmap to "RUNPROG.BMP"
        Set Size to 18 28
        Set Location to 202 227

        //AB-StoreStart
        Procedure OnClick
              if (psrep(self)='r100_000.rpt') begin
                 Erasefile "c:\ettik.txt"
                 Set Report_name  of (clients19WQLReport(current_object)) to "r100_000.rpt"
                 Send Runclients19Report To (clients19WQLReport(Current_Object))
                 Send diabase_kiniseis
              end
              else;
                  Send RunclientsReport To (clientsWQLReport(Current_Object))
        End_Procedure
        
        Procedure diabase_kiniseis
            string sid sname stk
            integer iarea ideikths i1 iint
        
            Direct_Input Channel 9 'C:\ettik.txt'
            While (Not(Seqeof))
                Read    Channel 9 sid sname
                If (Trim(sid)='') Goto Telos_Edo_Giorgos19
                Clear pagkat
                Move sid to pagkat.pagkat_code
                Find Eq pagkat By Index.1
                if (not(found)) begin
                    move (ToOEM (sNAME)) to pagkat.pagkat_descr
                    reread sysrecg
                    move (sysrecg.pagkat_rec0+1) to sysrecg.pagkat_rec0
                    saverecord sysrecg
                    unlock
                    move  sysrecg.pagkat_rec0 to pagkat.pagkat_rec0
                    Move (Append('',((Trim(SysCurXR.Branch_Site))+;
                         (Trim(String(pagkat.pagkat_Rec0)))))) to pagkat.pagkat_Recr
                    Saverecord pagkat
                    get piint to iint
                    Set Array_Value of (Selected_Items(Ctypkin_Grid(self))) Item iInt to Pagkat.Recnum
                    Add 1 to iInt
                    set piint to iint
                    Set Value of (frmCount(self)) item 0 to (Select_Count(Ctypkin_Grid(Self)))
                    Send Beginning_Of_Data    to (Ctypkin_Grid(self))
                end
                Telos_Edo_Giorgos19:
                Readln Channel 9
            Loop
        
             Close_output Channel 9
        
        End_procedure
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
