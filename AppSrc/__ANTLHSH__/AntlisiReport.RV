    Procedure ProduceTheTextFile
        Local string    sClientCode sCNCode  sItemCode  sCategoryCode sHmgioCode sstr sbranch scode skat sbr sEktypCode
        Local string    sClientName sCNDescr sItemDescr sCategoryDescr sHmgioDescr dHmniaApo dHmniaEos sname sSeira srep sEktypDescr
        Local string    endtypkinn FileName flysh1 flysh2 slabel shmgio stext Selection
        Local Integer   Destination SortOrder NumSortFields CurSortField repstatus FileType
        Local Integer   SortDirection wdate fepil starttypkinn iint icheck icount irec iSX iAnal
        date            apo eos
    
        move "" to sstr
        move "" to sBranch
        move "" to sText
    
        Get Select_Count of (dblstbranch(Self)) to iInt
    
        If (iInt>0) Begin
            move '' to stext
            move '' to sBranch
            For iCount from 0 to (iInt-1)
                Get array_value of (Selected_Items(dblstbranch(self))) Item iCount to iRec
                Clear branch
                Move iRec to branch.Recnum
                Find Eq branch.Recnum
                move (Trim(branch.branch_code)) to scode
                move (Trim(branch.branch_name)) to sname
                Move (Append(sbranch,(trim(sname)+' '))) to sbranch
                Move (Append(stext  ,(trim(scode)+' '))) to stext
            Loop
        End
    
        Get piAnal                              to iAnal
        get psModule                            to flysh1
        Move (Trim(sBranch))                    to sBranch
        Move (Trim(sText  ))                    to sText
        Get Value Of frmHmApo           Item 0  to dHmniaApo
        Get Value Of frmHmEos           Item 0  to dHmniaEos
        Get Value Of frmEktypCode       Item 0  to sEktypCode
        Get Value Of frmEktypDescr      Item 0  to sEktypDescr
    
        Get WQLFileName                         to FileName
    
        Clear Wcmoves
        Move dHmniaApo to Wcmoves.Wcmove_Hmnia
        Find GE Wcmoves By Index.3
        While ((Found) And (Wcmoves.Wcmove_Hmnia<=dHmniaEos))
    
            Set Label of btnMess to ("Ä§òùû´È úöö®ò≠‚™ ò§´¢†È§...  "+Trim(Wcmoves.Wcmove_Hmnia))
    
            If ((sText Contains Trim(WCMOVES.BRANCH_CODE)) And (Trim(WCMOVES.WITEMKAT_CODE)='05')) Begin
    
                Clear Pumps
                Clear Pmoves
                Clear Tmplg
                Clear Particle
                Clear Branch
    
                Move Wcmoves.Company_Code   to Branch.Company_Code
                Move Wcmoves.Branch_Code    to Branch.Branch_Code
                Find Eq branch By Index.1
    
                Move Wcmoves.Partl_Recr     to Pmoves.Pms_Recr
                Find Eq Pmoves By Index.2
                If (Not(Found)) Error 5501
    
                Move Pmoves.Partl_Recr     to Particle.Partl_Recr
                Find Eq Particle By Index.2
                If (Not(Found)) Error 5502
    
                Move Pmoves.Pump_ID         to Pumps.Pump_ID
                Find Eq Pumps By Index.3
                //If ((Trim(Pmoves.Pump_ID)<>"") And (Found=0)) Error 5502
    
                If (iAnal) Begin
    
                    // Deal with Tmplg unique index
                    Move (Trim(Wcmoves.Branch_Code))                to Tmplg.Company_Code
                    Move Wcmoves.Wcmove_Hmnia                       to Tmplg.Branch_Code
                    Move Pumps.Ctype_ID                             to Tmplg.Year_Year
                    Move (Trim(Wcmoves.Wcmove_Recr))                to Tmplg.Gtypkin_Recr
                    Move (Trim(Pumps.Pump_License))                 to Tmplg.Gplan_Code
    
                         if (PUMPS.CTYPE_ID='1') Move "à.ï."        to Tmplg.Gmove_A_Code
                    else if (PUMPS.CTYPE_ID='2') Move "É.ï."        to Tmplg.Gmove_A_Code
                    else if (PUMPS.CTYPE_ID='3') Move "ÄåÑì"        to Tmplg.Gmove_A_Code
                    else if (PUMPS.CTYPE_ID='4') Move "ÑêÇéäÄÅóå"   to Tmplg.Gmove_A_Code
    
                    Move Pmoves.Pump_ID                             to Tmplg.Gmove_SC_Code2
    
                    Move Branch.Egkatastash                         to Tmplg.Gmove_KK
                    Move Branch.Perioxh                             to Tmplg.Gmove_Recr
                    Move Particle.Ptyppar_Code                      to Tmplg.Year_Recr
    
                    Move Wcmoves.Wcmove_QTY                         to Tmplg.Gmove_X0
                    Move Wcmoves.SX_Posothta_EX                     to Tmplg.Gmove_P0
                    //Move Wcmoves.Wcmove_Axia                        to Tmplg.Gmove_X0K
                    // Apoforologhmeno  19/09/2011
                    Move Wcmoves.Wcmov_Totamad0                     to Tmplg.Gmove_X0K
                    Move (Trim(Wcmoves.Wcmove_Par))                 to Tmplg.Gmove_Aitiol
    
                    Saverecord Tmplg
                End
                Else Begin
                  Integer isFound
                  Move 0 to isFound
    
    
                    // Deal with Tmplg unique index
                    Move (Trim(Wcmoves.Branch_Code))                to Tmplg.Company_Code
                    Move Wcmoves.Wcmove_Hmnia                       to Tmplg.Branch_Code
                    Move Pumps.Ctype_ID                             to Tmplg.Year_Year
                    //Move (Trim(Wcmoves.Wcmove_Recr))                to Tmplg.Gtypkin_Recr
                    Move (Trim(Pumps.Pump_License))                 to Tmplg.Gplan_Code
    
                    Find Eq Tmplg By Index.1
                    Move (Found) to isFound
    
                    If (isFound) Reread Tmplg
    
                         if (PUMPS.CTYPE_ID='1') Move "à.ï."        to Tmplg.Gmove_A_Code
                    else if (PUMPS.CTYPE_ID='2') Move "É.ï."        to Tmplg.Gmove_A_Code
                    else if (PUMPS.CTYPE_ID='3') Move "ÄåÑì"        to Tmplg.Gmove_A_Code
                    else if (PUMPS.CTYPE_ID='4') Move "ÑêÇéäÄÅóå"   to Tmplg.Gmove_A_Code
    
                    Move Pmoves.Pump_ID                             to Tmplg.Gmove_SC_Code2
    
                    Move Branch.Egkatastash                         to Tmplg.Gmove_KK
                    Move Branch.Perioxh                             to Tmplg.Gmove_Recr
                    Move Particle.Ptyppar_Code                      to Tmplg.Year_Recr
    
                    Add Wcmoves.Wcmove_QTY                          to Tmplg.Gmove_X0
                    Add Wcmoves.SX_Posothta_EX                      to Tmplg.Gmove_P0
    
                    //Move Wcmoves.Wcmove_Axia                        to Tmplg.Gmove_X0K
                    // Apoforologhmeno  19/09/2011
                    Move Wcmoves.Wcmov_Totamad0                     to Tmplg.Gmove_X0K
    
                    //Move (Trim(Wcmoves.Wcmove_Par))                 to Tmplg.Gmove_Aitiol
    
                    Saverecord Tmplg
    
                    If (isFound) Unlock
                End
            End
            Find GT Wcmoves By Index.3
        Loop
    
        Direct_Output Channel 8 FileName
        Clear Tmplg
        Find GE Tmplg By Index.1
        While (Found)
    
    //    Tmplg.Gmove_KK      // Egkatast
    //    Tmplg.Gmove_Recr    // Perioxh
    //    Tmplg.Year_Recr     // Ptyppar_Code
    //    Tmplg.Branch_Code   // Hmnia
    //    Tmplg.Gmove_A_Code  // Ctype Description
    //    Tmplg.Gplan_Code    // Pump License Plate
    //    Tmplg.Gmove_X0      // Antlhmena M3
    //    Tmplg.Gmove_P0      // Timologhmena M3
    //    Tmplg.Gmove_X0K     // Timologhmenh Axia
    //    Tmplg.Gmove_Aitiol  // Parastatiko
    
    
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(Tmplg.Gmove_KK))      // Egkatastash
            Write   Channel 8       '"'
            Write   Channel 8       ','
    
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(Tmplg.Gmove_Recr))    // Perioxh
            Write   Channel 8       '"'
            Write   Channel 8       ','
    
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(Tmplg.Year_Recr))     // Ptyppar _Code
            Write   Channel 8       '"'
            Write   Channel 8       ','
    
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(Tmplg.Branch_Code))   // Hmnia
            Write   Channel 8       '"'
            Write   Channel 8       ','
    
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(Tmplg.Gmove_A_Code))  // Ctype Descr
            Write   Channel 8       '"'
            Write   Channel 8       ','
    
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(Tmplg.Gplan_Code))    // Pump License
            Write   Channel 8       '"'
            Write   Channel 8       ','
    
            Move (String(Tmplg.Gmove_X0)) to sStr
            Move (Replaces(",",sStr,""))  to sStr
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(sStr))     // QTY  ANTLHMENA
            Write   Channel 8       '"'
            Write   Channel 8       ','
    
            Move (String(Tmplg.Gmove_P0)) to sStr
            Move (Replaces(",",sStr,""))  to sStr
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(sStr))     // SX_POSOTHTA_EX / QTY TIMOLOGHMENA
            Write   Channel 8       '"'
            Write   Channel 8       ','
    
            Move (String(Tmplg.Gmove_X0K)) to sStr
            Move (Replaces(",",sStr,""))   to sStr
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(sStr))     // AXIA / TIMOLOGHMENA AXIA
            Write   Channel 8       '"'
            Write   Channel 8       ','
    
            String sTmp
            Move (Trim(Tmplg.Gmove_Aitiol)) to sTmp
            Move (Replaces("*",sTmp,""))    to sTmp
            Write   Channel 8       '"'
            Write   Channel 8       (Trim(sTmp))  // Parastatiko AFAIROUME ta ***
            Write   Channel 8       '"'
    
            Writeln Channel 8
    
            Find GT Tmplg By Index.1
        Loop
        Close_Output Channel 8
    
    
        Copyfile FileName to (FileName+".Keep")
    
        Send ExportAsSapFormat
    
        Set Label of btnMess to ""
    
    End_Procedure
    
