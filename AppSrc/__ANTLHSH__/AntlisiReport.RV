    Procedure ExportAsSapFormat
        String FileName sRep vs1 vs2 vs3 vs4 vs5 vs6 vs7 vs8 vs9 vs10
        String sArxeio sEgkatas sPoso sMonth sYear
        Integer iAnal iPos iLength iDiafora iLen iInt
        Number nPoso
        Date sHmnia
    
        Get WQLFileName to FileName
        If (Trim(FileName)="") Begin
            Send Stop_Box "Éú§ ô®Â©°‡ ´¶ ò®ÆúÂ¶ £ú ´†™ úöö®ò≠‚™" "ë≠·¢£ò 5001"
            Procedure_return
        End
    
        Clear Branch
        Move SyscurXr.Company_Code      to Branch.Company_Code
        Move SyscurXr.Branch_Code       to Branch.Branch_Code
        Find Eq Branch By Index.1
        Move (Trim(Branch.Egkatastash)) to sEgkatas
    
        Get Value Of frmHmApo Item 0    to sHmnia
        Move (TheMonthSegment(sHmnia))  to sMonth
        If (Length(sMonth)=1) Insert "0" in sMonth at 1
        Move (TheYearSegment(sHmnia))   to sYear
    
        Move ("C:\"+Trim(sEgkatas)+"PUMP"+Trim(sMonth)+Trim(sYear)+".TXT") to sArxeio
    
        Direct_Output Channel 7 sArxeio
    
        Direct_Input Channel 8 FileName
        Readln Channel 8 vs1 vs2 vs3 vs4 vs5 vs6 vs7 vs8 vs9 vs10
        While (Not(Seqeof))
    
            Set Label of btnMess to ("î´†·Æ§‡ ´¶ ò®ÆúÂ¶ ´Áß¶¨ SAP...  "+Trim(vs2)+"  "+Trim(vs6))
    
            Move (Trim(vs1 )) to vs1   // Egkatastash
            Move (Trim(vs2 )) to vs2   // Perioxh
            Move (Trim(vs3 )) to vs3   // Ptyppar
            Move (Trim(vs4 )) to vs4   // Hmnia
            Move (Trim(vs5 )) to vs5   // Eidos Antlias (IX DX)
            Move (Trim(vs6 )) to vs6   // Pinakida
            Move (Trim(vs7 )) to vs7   // Antlhthenta  M3
            Move (Trim(vs8 )) to vs8   // Timologhmena M3
            Move (Trim(vs9 )) to vs9   // Timologhmenh Axia
            Move (Trim(vs10)) to vs10  // Parastatiko (Wcmoves_Par)
            //--
            Move (Replaces("/",vs4 ,"")) to vs4
            Move (Replaces('"',vs10,"")) to vs10
            //--
    //        Move (Replaces(".",vs7,"")) to vs7
    //        Move (Replaces(",",vs7,"")) to vs7
    //        Move (Replaces(".",vs8,"")) to vs8
    //        Move (Replaces(",",vs8,"")) to vs8
    //        Move (Replaces(".",vs9,"")) to vs9
    //        Move (Replaces(",",vs9,"")) to vs9
    
    
            Move vs7                                             to nPoso
            Move nPoso                                          to sPoso
            Move (Trim(sPoso))                                  to sPoso
            Move (Pos('.',sPoso))                               to iPos
            Move (Length(sPoso))                                to iLength
            Move (iLength-iPos)                                 to iDiafora
            If (Pos('.',sPoso)=0)   Move (Append(sPoso,'.00' )) to sPoso
            Else If (iDiafora =1)   Move (Append(sPoso,'0'   )) to sPoso
            //Else If (iDiafora =2)   Move (Append(sPoso,'0'   )) to sPoso
            Move (Replace('.',sPoso,''))                        to sPoso
            Move (Length(sPoso))                                to iLen
            For iInt From 1 to (10-iLen)
                Move (Insert("0", sPoso,1))                     to sPoso
            Loop
            Move (Trim(sPoso))                                  to vs7
    
            Move vs8                                            to nPoso
            Move nPoso                                          to sPoso
            Move (Trim(sPoso))                                  to sPoso
            Move (Pos('.',sPoso))                               to iPos
            Move (Length(sPoso))                                to iLength
            Move (iLength-iPos)                                 to iDiafora
            If (Pos('.',sPoso)=0)   Move (Append(sPoso,'.00' )) to sPoso
            Else If (iDiafora =1)   Move (Append(sPoso,'0'   )) to sPoso
            //Else If (iDiafora =2)   Move (Append(sPoso,'0'   )) to sPoso
            Move (Replace('.',sPoso,''))                        to sPoso
            Move (Length(sPoso))                                to iLen
            For iInt From 1 to (10-iLen)
                Move (Insert("0", sPoso,1))                     to sPoso
            Loop
            Move (Trim(sPoso))                                  to vs8
    
            Move vs9                                            to nPoso
            Move nPoso                                          to sPoso
            Move (Trim(sPoso))                                  to sPoso
            Move (Pos('.',sPoso))                               to iPos
            Move (Length(sPoso))                                to iLength
            Move (iLength-iPos)                                 to iDiafora
            If (Pos('.',sPoso)=0)   Move (Append(sPoso,'.00'))  to sPoso
            Else If (iDiafora =1)   Move (Append(sPoso,'0'  ))  to sPoso
            Move (Replace('.',sPoso,''))                        to sPoso
            Move (Length(sPoso))                                to iLen
            For iInt From 1 to (10-iLen)
                Move (Insert("0", sPoso,1))                     to sPoso
            Loop
            Move (Trim(sPoso))                                  to vs9
            //--
            Write   Channel 7 (Pad(Trim(vs1 )  , 4)) // Egkatastash
            Write   Channel 7 (Pad(Trim(vs2 )  , 4)) // Perioxh
            Write   Channel 7 (Pad(Trim(vs3 )  , 3)) // Ptyppar
            Write   Channel 7 (Pad(Trim(vs4 )  , 8)) // Hmnia
            Write   Channel 7 (Pad(Trim(vs5 )  , 2)) // Eidos Antlias (IX DX)
            Write   Channel 7 (Pad(Trim(vs6 )  ,12)) // Pinakida
            Write   Channel 7 (Pad(Trim(vs7 )  ,10)) // Antlhthenta  M3
            Write   Channel 7 (Pad(Trim(vs8 )  ,10)) // Timologhmena M3
            Write   Channel 7 (Pad(Trim(vs9 )  ,10)) // Timologhmenh Axia
            Write   Channel 7 (Pad(Trim(vs10)  ,20)) // Parastatiko (Wcmoves_Par)
            Writeln Channel 7
            //--
            Readln Channel 8 vs1 vs2 vs3 vs4 vs5 vs6 vs7 vs8 vs9 vs10
        Loop
    
        Close_Output Channel 7
        Close_Input  Channel 8
    
        Set Label of btnMess to ""
    
        Date dNoMatter
        Sysdate dNoMatter
    
        Runprogram Wait "C:\VDF6\Develop\Empor\Programs\Convert_To_UTF.Exe" (String(dNoMatter)+" "+Trim(sArxeio)+" "+"1")
    
        EraseFile "C:\Antlisi_Stat.csv"
    
        Send Info_Box ("í¶ ò®ÆúÂ¶ úÂ§ò† ‚´¶†£¶ ©´¶ ´¶ß†°Ê õÂ©°¶ C ‡™ "+Trim(sArxeio)) "è¢û®¶≠¶®Âò"
