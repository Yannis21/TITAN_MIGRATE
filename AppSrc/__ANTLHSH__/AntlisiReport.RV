            Set Formula "formula1" to (Winql_String(psModule(Self)))
            Set Formula "flabel"   to (Winql_String(sEktypDescr))
            Set Formula "fwhat"    to (Winql_number(piwhat(Self)))
            Set Formula "fbranch"  to (Winql_String(sBranch))
    
            Set Formula "HideDetails"   to (Winql_number(1 - piAnal(Self)))
    
            get epil                    to fepil
            Set Formula "fepil"         to (winql_number(fepil))
    
            Get WQLFileName             to FileName
    
    //        If (piPrintWhere(Self)=2) Begin
    //            Set Output_Device_Mode          To WQL_PRINT_TO_FILE
    //            Set Output_Destination          to WQL_PRINT_TO_FILE
    //            Set ExportFileName              to (Trim(FileName))
    //            Set ExportFormatType            to WQL_CharSeparatedType
    //            Set FieldDelimiter              to ";"
    //            Set StringDelimiter             to '"'
    //            Set UseReportNumberFormat_State to True
    //            Set UseReportDateFormat_State   To True
    
    //WQL_CommaSeparatedType
    //WQL_CharSeparatedType
    //WQL_TabSeparatedType
    
            //End
            //Else Begin
               Set Output_Device_Mode To WQL_PRINT_TO_WINDOW
               Set Output_Destination to WQL_PRINT_TO_WINDOW
            //End
    
            Append Selection (If(Selection <> "", " and ", ""))"{WCMOVES.YEAR_YEAR} = " (WinQL_Number(syscurxr.year_year))
            Append Selection (If(Selection <> "", " and ", "")) "InStr('" sText "',{WCMOVES.BRANCH_CODE})<>0"
            if (dHmniaApo<>"") Append Selection (If(Selection <> "", " and ", ""))"{WCMOVES.WCMOVE_HMNIA} >= " (winql_date(dHmniaApo))
            if (dHmniaEos<>"") Append Selection (If(Selection <> "", " and ", ""))"{WCMOVES.WCMOVE_HMNIA} <= " (winql_date(dHmniaEos))
            Append Selection (If(Selection <> "", " and ", ""))"{WCMOVES.WITEMKAT_CODE} = " '"' (trim("05")) '"'
    
