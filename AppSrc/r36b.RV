        Procedure Activate_View Returns Integer
            date ddate
            Integer rVal iint icheck
            string scompany
            set value of (fenarxh(groupsub4(self))) to '01/01/2000'
            set value of (flhxh(groupsub4(self))) to syscurxr.year_xrhshto
            send rebuild_constraints to baxiokat_dd
            send beginning_of_data to dblstbaxiokat true
