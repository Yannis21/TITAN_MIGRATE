        Object axioWQLReport Is A crystalreportSubSQL
        
            Property Integer ReportReady Public False
            Set Report_View_id To (Parent(Current_object))
        
            Procedure OnStartPrinting
                integer iint1 icount1 irec1 icode
                date  apo eos  enarxh lhxh
                local string startnumbern startslsn startprespn sapo smonth
                Local string  Selection FileName slabel sbranch sone stext
                Local integer Starthmen Endhmen stbaxio FileType Destination ;
                              SortOrder NumSortFields CurSortField SortDirection wdate;
                              lhxhhmen enarxhhmen
                Integer iint icheck icount irec
                string  scode sname
                move '1' to sone
                Move "" To Selection
                get starthme       to starthmen
                get startnumber    to startnumbern
                get startsls       to startslsn
                get startpresp     to startprespn
                get enarxhhme      to enarxhhmen
                get lhxhhme        to lhxhhmen
                Append Selection (If(Selection <> "", " and ", ""))"{baxio.company_code} = " '"' (Trim(syscurxr.company_code)) '"'
                Get Select_Count of (dblstbranch(Self)) to iInt
                If (iInt>0) Begin
                    move '' to stext
                    For iCount from 0 to (iInt-1)
                        Get array_value of (Selected_Items(dblstbranch(self))) Item iCount to iRec
                        Clear branch
                        Move iRec to branch.Recnum
                        Find Eq branch.Recnum
                        move branch.branch_code to scode
                        move branch.branch_name to sname
                        Move (Append(sbranch,(trim(sname)+' '))) to sbranch
                        Move (Append(stext,(trim(scode)+' '))) to stext
                    Loop
                End
                Append Selection (If(Selection <> "", " and ", "")) "InStr('" stext "',{baxio.branch_code})<>0"
                Append Selection (If(Selection <> "", " and ", ""))"{baxio.baxio_rater} <> 1 "
            //    Append Selection (If(Selection <> "", " and ", ""))"{baxio.BAXIO_PLACE} <> 2 "
                Append Selection (If(Selection <> "", " and ", ""))"{btypkin.btypkin_anti_xp} = 2"
                Append Selection (If(Selection <> "", " and ", ""))"{btypkin.btypkin_sign} = 1"
        
                Append Selection (If(Selection <> "", " and ", ""))"{barticl.barticl_ekleise} = " '"' (Trim(sone)) '"'
                If enarxhhmen ne 0 ;
                    Append Selection (If(Selection <> "", " and ", "")) "{bArticl.barticl_hmnia} >= " '' (winql_date(enarxhhmen))''
                If lhxhhmen ne 0 ;
                    Append Selection (If(Selection <> "", " and ", "")) "{bArticl.barticl_hmnia} <= " '' (winql_date(lhxhhmen))''
        
                if (startslsn<>'') Append Selection (If(Selection <> "", " and ", ""))"{bmove.sls_id} = " '"' (Trim(startslsn)) '"'
                if (startprespn<>'') Append Selection (If(Selection <> "", " and ", ""))"{bmove.presp_id} = " '"' (Trim(startprespn)) '"'
                if (startnumbern<>'') Append Selection (If(Selection <> "", " and ", ""))"{baxio.baxio_code_in} = " '"' (Trim(startnumbern)) '"'
                If starthmen ne 0 ;
                      Append Selection (If(Selection <> "", " and ", "")) "{bAXIO.baxio_hmnial} >= " '' (winql_date(starthmen))''
                Get Select_Count of (dblstbaxiokat(Self)) to iInt1
                If (iInt1>0) Begin
                    move '' to stext
                    For iCount1 from 0 to (iInt1-1)
                        Get array_value of (Selected_Items(dblstbaxiokat(self))) Item iCount1 to iRec1
                        Clear baxiokat
                        Move iRec1 to baxiokat.Recnum
                        Find Eq baxiokat.Recnum
                        move baxiokat.baxiokat_code to icode
                        move (Append(stext,'#'+(String(icode)))+'#') to stext
                    Loop
                end
                Append Selection (If(Selection <> "", " and ", "")) "InStr('" stext "',('#'+TOTEXT ({baxio.baxiokat_code},0)+'#'))<>0"
                Set SelectionFormula To Selection
                If Selection Eq "" ;
                    Set Formula "SelTxt" To ('"' + "NONE" + '"')
                Else ;
                    Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
        
                get enarxhhme       to enarxh
                get lhxhhme         to lhxh
                get starthme        to apo
                set formula "fenarxh" To (winql_date(enarxh))
                set formula "flhxh" To (winql_date(lhxh))
                Set Formula "fapo"  To (winql_date(apo))
                set formula "fbranch" to (winql_string(sbranch))
                get value of form3  to slabel
                Set Formula "flabel" to (Winql_String(slabel))
                Get NSortFields To NumSortFields
                For CurSortField From 0 To (NumSortFields - 1)
                Send DeleteNthSortField 0
                Loop
                move PE_SF_asCENDING to sortdirection
                Get Output_Destination To Destination
                If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                    Get WQLFileName To FileName
                    Get WQLFileType To FileType
                    Set ExportFileName   To FileName
                    Set ExportFormatType To FileType
                End
                Else If Destination Eq WQL_PRINT_TO_PRINTER ;
                Send SelectAndSetPrinter
                If Destination EQ WQL_PRINT_TO_WINDOW ;
                    Set ReportReady To False
                Else ;
                    Set ReportReady To True
            End_procedure // OnStartPrinting
        
            Procedure RunaxioReport
                Send Cursor_Wait To (Cursor_Control(Current_Object))
                Set Report_name  To (psrep(self))
                Send Run_report
                Send Cursor_Ready To (Cursor_Control(Current_Object))
                If (ReportReady(Current_object)) ;
                    Send Info_box "WinQL report ready."
            End_procedure // RunCustReport
        
        End_object // CustWQLReport
        
