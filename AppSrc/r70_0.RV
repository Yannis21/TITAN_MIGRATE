    Object axioWQLReport Is A crystalreportSubSQL
    
        Property Integer ReportReady Public False
        Set Report_View_id To (Parent(Current_object))
    
        Procedure OnStartPrinting
            date datef datet apo eos
            Local string  Stclient stpayresp stcn  Selection FileName smodule;
                          sstr flysh1 flysh2 str Str2 sbranch slabel
            Local integer Starthmeff Endhmeff starthmepp endhmepp stbaxio;
                          starthmekk endhmekk FileType Destination sepil;
                          SortOrder NumSortFields CurSortField SortDirection wdate
            get psModule        to sModule
            Move "" To Selection
            Get StartNumber     To Stclient
            Get startpayresp    to stpayresp
            get startcn         to stcn
            get startbaxio      to stbaxio
            get starthmef       to starthmeff
            Get Endhmef         To Endhmeff
            get starthmep       to starthmepp
            Get Endhmep         To Endhmepp
            get starthmek       to starthmekk
            Get Endhmek         To Endhmekk
            get psbranch        to sbranch
            Move '' to Str2
            Append Selection (If(Selection <> "", " and ", ""))"{particle.company_code} = " '"' (Trim(syscurxr.company_code)) '"'
            if (sbranch<>'') Append Selection (If(Selection <> "", " and ", ""))"{particle.branch_code} = " '"' (Trim(sbranch)) '"'
            Append Selection (If(Selection <> "", " and ", "")) "{particle.year_year} = " '' (syscurxr.year_year) ''
    
            Append Str2 "(IsNull({TPARTICL.ISOK}) Or {TPARTICL.ISOK}='')"
            Append Selection (If(Selection <> "", " and ", "")) Str2
            If (trim(Stclient)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "Trimright({particle.client_id}) = " '"' (trim(stclient)) '"'
    
            If (trim(stpayresp)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "Trimright({particle.presp_id}) = " '"' (trim(stpayresp)) '"'
    
            If (trim(stcn)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "Trimright({particle.cn_code}) = " '"' (trim(stcn)) '"'
            If stbaxio ne 0 ;
               Append Selection (If(Selection <> "", " and ", "")) "{tparticl.baxiokat_code} = " '' (stbaxio) ''
    
            If (trim(starthmeff)) ne 0 ;
                Append Selection (If(Selection <> "", " and ", "")) "{particle.partl_hmnia} >= " (winql_date(starthmeff))
            If (trim(Endhmeff)) ne 0 ;
                  Append Selection (If(Selection <> "", " and ", "")) "{particle.partl_hmnia} <= " (winql_date(Endhmeff))
    
            If (trim(starthmepp)) ne 0 ;
                Append Selection (If(Selection <> "", " and ", "")) "{tparticl.paralabh} >= " (winql_date(starthmepp))
            If (trim(Endhmepp)) ne 0 ;
                  Append Selection (If(Selection <> "", " and ", "")) "{tparticl.paralabh} <= " (winql_date(Endhmepp))
            If (trim(starthmekk)) ne 0 ;
                Append Selection (If(Selection <> "", " and ", "")) "{tparticl.exoflhsh} >= " (winql_date(starthmekk))
            If (trim(Endhmekk)) ne 0 ;
                  Append Selection (If(Selection <> "", " and ", "")) "{tparticl.exoflhsh} <= " (winql_date(Endhmekk))
    
            get psmodule to flysh1
            move '04' to flysh2
            Move '' to sStr
            Move (Append(sStr,(flysh1+flysh2))) to sStr
            Move (Trim(sStr)) to sStr
            Append Selection (If(Selection <> "", " and ", "")) "InStr('" sStr "',{particle.partl_module})<>0"
            Set SelectionFormula To Selection
            If Selection Eq "" ;
                Set Formula "SelTxt" To ('"' + "NONE" + '"')
            Else ;
                Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
            clear branch
            move syscurxr.company_code to branch.company_code
            move sbranch               to branch.branch_code
            find eq branch by index.1
            Set Formula "fbranch" to (Winql_String(branch.branch_name))
            get  epil70 to sepil
            get starthmef to apo
            get endhmef   to eos
            Set Formula "fepil" to (winql_number(sepil))
            Set Formula "fapo"  To (winql_date(apo))
            Set Formula "feos"  To (winql_date(eos))
            get starthmep to apo
            get endhmep   to eos
            if (apo = '') move syscurxr.year_xrhshfrom to apo
            if (eos = '') move syscurxr.year_xrhshto   to eos
            Set Formula "fparaapo"  To (winql_date(apo))
            Set Formula "fparaeos"  To (winql_date(eos))
            Get NSortFields To NumSortFields
            For CurSortField From 0 To (NumSortFields - 1)
            Send DeleteNthSortField 0
            Loop
            get descasc to sortdirection
            set GroupField PE_GROUPHEADER to "{baxiokat.baxiokat_code}"
            Send AddSortField "{tparticl.exoflhsh}" SortDirection
            set formula "formula1" to (winql_string(smodule))
            set formula "fwhat" to (winql_number(piwhat(self)))
            get value of form3 to slabel
            set formula "flabel" to (winql_string(slabel))
                    //*** Handle export to file
            Get Output_Destination To Destination
            If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                Get WQLFileName To FileName
                Get WQLFileType To FileType
    
                Set ExportFileName   To FileName
                Set ExportFormatType To FileType
            End
            Else If Destination Eq WQL_PRINT_TO_PRINTER ;
            Send SelectAndSetPrinter
            If Destination EQ WQL_PRINT_TO_WINDOW ;
                Set ReportReady To False
            Else ;
                Set ReportReady To True
        End_procedure // OnStartPrinting
    
    
        Procedure RunaxioReport
            Send Cursor_Wait To (Cursor_Control(Current_Object))
            Set Report_name  To (psrep(self))
            Send Run_report
            Send Cursor_Ready To (Cursor_Control(Current_Object))
            If (ReportReady(Current_object)) ;
                Send Info_box "WinQL report ready."
        End_procedure // RunCustReport
    
    End_object // CustWQLReport
    
    
    
