        Object btnRun is a ButtonSub

            //AB-StoreTopStart
                    
            //AB-StoreTopEnd

            Set Bitmap to "printer1.BMP"
            Set Size to 26 31
            Set Location to 165 311
            Set Form_TypeFace Item 0 to "MS Sans Serif"

            //AB-StoreStart
                    
            Set TooltipValue to "„§œ¥œ¨š˜©å˜ „šš¨˜­é¤ ¡˜  „¡«ç§à©ž"
            
            Procedure Fill_The_Temp
                    Local string    sClientCode sCNCode  sItemCode  sCategoryCode sHmgioCode sHmerologio sbranch scode skat sbr sEktypCode
                    Local string    sClientName sCNDescr sItemDescr sCategoryDescr sHmgioDescr dHmniaApo dHmniaEos sname sSeira srep sEktypDescr
                    Local string    endtypkinn Selection FileName sModule flysh2 slabel shmgio stext iSelected iSelections iSel sText2
                    Local Integer   Destination SortOrder NumSortFields CurSortField repstatus FileType iSkip iSkipDet
                    Local Integer   SortDirection wdate fepil starttypkinn iint icheck icount irec iSX
                    Local String    sCmovesRecr sPmovesRecr sParticleRecr sWcmovesRecr sWcmPartlRecr sWcmPmsRecr
                    date            apo eos
                    Integer         bExeiAxia bExeiPosothta
            
                    Zerofile tmplg
            
                    move "" to sBranch
                    move "" to sText
                    move "" to sText2
                    Move "" To Selection
            
                    get  psModule                           to sModule
                    Get Value Of frmHmApo           Item 0  to dHmniaApo
                    Get Value Of frmHmEos           Item 0  to dHmniaEos
                    Get Value Of frmClientCode      Item 0  to sClientCode
                    Get Value Of frmClientName      Item 0  to sClientName
                    Get Value Of frmCNCode          Item 0  to sCNCode
                    Get Value Of frmCNDescr         Item 0  to sCNDescr
                    Get Value Of frmEktypCode       Item 0  to sEktypCode
                    Get Value Of frmEktypDescr      Item 0  to sEktypDescr
                    Get Value Of frmSeira           Item 0  to sSeira
            
                    Move (Trim(sModule      )) to sModule
                    Move (Trim(dHmniaApo    )) to dHmniaApo
                    Move (Trim(dHmniaEos    )) to dHmniaEos
                    Move (Trim(sClientCode  )) to sClientCode
                    Move (Trim(sClientName  )) to sClientName
                    Move (Trim(sCNCode      )) to sCNCode
                    Move (Trim(sCNDescr     )) to sCNDescr
                    Move (Trim(sEktypCode   )) to sEktypCode
                    Move (Trim(sEktypDescr  )) to sEktypDescr
                    Move (Trim(sSeira       )) to sSeira
            
                    If (dHmniaApo = "") Move SyscurXR.Year_XrhshFrom to dHmniaApo
                    If (dHmniaEos = "") Move SyscurXR.Year_XrhshTo   to dHmniaEos
            
                    Get Select_Count of (dblstbranch(Self)) to iInt
            
                    If (iInt>0) Begin
                        Move "" to sBranch
                        Move "" to sText
            
                        For iCount from 0 to (iInt-1)
                            Get array_value of (Selected_Items(dblstbranch(self))) Item iCount to iRec
                            Clear branch
                            Move iRec to branch.Recnum
                            Find Eq branch.Recnum
                            move (Trim(branch.branch_code)) to scode
                            move (Trim(branch.branch_name)) to sname
                            Move (Append(sbranch,(trim(sname)+' '))) to sbranch
                            Move (Append(stext  ,(trim(scode)+' '))) to stext
                        Loop
                    End
            
                    Move 0 to iSkip
                    Move 0 to iSkipDet
            
                    Integer iExitLoop iExits iSignW iSignC iPrevNo
            
                    Move 0 to iExitLoop
                    Move 0 to iSelected
                    Move 0 to iSelections
                    Move 0 to iSel
                    Move 0 to iExits
            
                    // MAIN PROCESS START ------------------------
                    // Start Filtering Particle
                    Clear Particle
                    Move dHmniaApo to Particle.Partl_Hmnia
                    Find GE Particle by Index.3 // Partl_Hmnia + Partl_Recr
                    While ((Found) And (iExitLoop=0))
            
                        Set Label of btnMess to (String(Particle.Partl_Hmnia)+"  "+Particle.Partl_Rec0)
            
                        Move -99999 to iPrevNo
            
                        If (Particle.Partl_Hmnia<=dHmniaEos) Begin
                            If (Particle.Company_Code <> syscurxr.company_code)                     Add 1 to iSkip
                            If (Particle.Year_Year    <> syscurxr.Year_Year   )                     Add 1 to iSkip
                            If (Particle.Partl_Module <> sModule              )                     Add 1 to iSkip
                            If ((sSeira     <>"") And (Particle.Pseira_Seira<>sSeira     ))         Add 1 to iSkip
                            If ((sClientCode<>"") And (Particle.Client_ID   <>sClientCode))         Add 1 to iSkip
                            If ((sCNCode    <>"") And (Particle.CN_Code     <>sCNCode    ))         Add 1 to iSkip
                            If ((Trim(sText)<>"") And (NOT(sText Contains (Trim(Particle.Branch_Code))))) ;
                                                                                                    Add 1 to iSkip
            
                            If (iSkip=0) Begin
                                Move "" to sCmovesRecr
                                Move "" to sPmovesRecr
                                Move "" to sParticleRecr
                                Move "" to sWcmovesRecr
                                Move "" to sWcmPartlRecr
                                Move "" to sWcmPmsRecr
            
                                Move Particle.Partl_Recr    to sParticleRecr
                                Move Particle.Cmoves_Recr   to sCmovesRecr
            
                                Move 1 to iSignC
                                Move 0 to bExeiAxia
            
                                Clear Cmoves
                                Move sCmovesRecr to Cmoves.Cmoves_Recr
                                Find Eq Cmoves By Index.2
            
                                Move ((Found) And (Cmoves.Cmoves_Axia<>0))          to bExeiAxia
                                If   ((Found) And (Cmoves.Cmoves_Axia< 0)) Move -1  to iSignC
            
                                // Pmoves Checking
                                Clear Pmoves
                                Move sParticleRecr to Pmoves.Partl_Recr
                                Find GE Pmoves By Index.3
                                While ((Found) And (sParticleRecr=Pmoves.Partl_Recr))
            
                                    If ((iSkip=0) And (Trim(Pmoves.Sx_Parast)<>"")) Begin
            
                                        Move 1 to iSignW
                                        Move 0 to bExeiPosothta
            
                                        Clear Wcmoves
                                        Move Pmoves.Wcmoves_Recr to Wcmoves.Wcmove_Recr
                                        Find Eq Wcmoves By Index.2
                                        Move ((Found) And (Wcmoves.Wcmove_Qty<>0)) to bExeiPosothta
                                        If ((Found) And (Wcmoves.Wcmove_Qty <0)) Move -1 to iSignW
            
                                        //==================================================================
                                        Relate Pmoves
            
                                        Clear Tmplg
            
                                        //----- Ayto kalyptei ton unique index tou arxeiou Tmplg
                                        Move Pmoves.Pms_Recr                    to Tmplg.Gtypkin_Recr
                                        Move Pmoves.Pms_Recr                    to Tmplg.Gplan_Recr
                                        // ----
                                        Move Particle.Partl_Recr                to Tmplg.Garticl_Recr
            
                                        // ------  Stoixeia ton sxetikon
                                        Move (Trim(Pmoves.Sx_Parast))           to Tmplg.Gmove_Aitiol
                                        Move Pmoves.Sx_Hmnia                    to Tmplg.Gmove_Dlixis
                                        Move Pmoves.Sx_Seira                    to Tmplg.Year_Recr
                                        Move Pmoves.Sx_Arithmos                 to Tmplg.Gmove_Open_YP2
            
                                        // ------  Stoixeia tou trexontos
                                        Move Particle.Partl_Hmnia               to Tmplg.Garticl_Hmnia
                                        Move Particle.Partl_No                  to Tmplg.Garticl_No
                                        Move Particle.Pseira_Seira              to Tmplg.Branch_Code
                                        Move (Trim(Ptyppar.Ptyppar_Descr))      to Tmplg.Baxio_Code_In
            
                                        // ------  Analytika Stoixeia tou kathe Eidous
                                        Move Pmoves.Item_Code                   to Tmplg.Gplan_Code
                                        Move Pmoves.Witemx_Descr                to Tmplg.Baxio_Code_Out
                                        Move Units.Units_Descr                  to Tmplg.Gmove_A_Code
                                        Move Wcmoves.Wapokat_Code               to Tmplg.Gmove_KK
            
                                        // ------  Enhmerosh Grammhs me Posothta / Axia
                                        If (bExeiPosothta) ;
                                            Move (Wcmoves.Wcmove_Qty)           to Tmplg.Gmove_X0K
                                        Else Move 0                             to Tmplg.Gmove_X0K
                                        Move Wcmoves.Wtypkin_Code               to Tmplg.Baxio_Fpar_Out
            
                                        If (WCMOVES.SX_POSOTHTA_EX=0) ;
                                                Move WCMOVES.wcmove_qty_exa     to Tmplg.Gmove_X0K
                                        Else    Move WCMOVES.SX_POSOTHTA_EX     to Tmplg.Gmove_X0K
            
            
                                        If (bExeiAxia) Begin
                                            Move  Pmoves.Pms_UnitPrice              to Tmplg.Gmove_P0K
                                            Move (Pmoves.Pms_AmAftDisc*iSignC)  to Tmplg.Gmove_X00
                                            Move (Pmoves.Pms_Vatam*iSignC)      to Tmplg.Gmove_P00
                                            Move (Pmoves.Pms_Payable*iSignC)    to Tmplg.Gmove_X00K
                                            if (Pmoves.Partl_No<>iPrevNo) Begin
