        Procedure Activate_View Returns Integer
            String sStr sModule StrDisp
            Integer rVal iint icheck
            string scompany
        
            set value of frmHmApo Item 0 to syscurxr.year_xrhshfrom
            set value of frmHmEos Item 0 to syscurxr.year_xrhshto
        
            Set Label of btnMess to ""
        
            Forward Get Msg_Activate_View to rVal
        
            Set piIsExcel Of DeliveryWQLReport to 0
        
            Get psMenuModule  Of Main_Menu  to sModule
            Set psModule                    to sModule
        
            Move (gsfCode_mast("MODULE",(psModule(Self)))) to    StrDisp
        
            Set Label Of DeliveryReportNew to "˜¨˜›æ©œ ª ‘¡¬¨¦›â£˜«¦ª / Delivery Of Concrete"
        
            Send Delete_Data            to (Selected_Items(dblstBRANCH(self)))
            Send Find to (branch_dd(self)) First_Record 1
            Set Dynamic_Update_State of (dblstbranch(self)) to false
            While (Found=1)
                get field_current_value of branch_dd field branch.company_code to scompany
                get field_current_value of branch_dd field branch.branch_apoth to icheck
                if ((syscurxr.company_code=scompany) and (icheck=1));
                    Set Array_Value of (Selected_Items(dblstbranch(self))) Item iInt to branch.Recnum
                Add 1 to iInt
                Send Find to (branch_dd(self)) Next_Record 1
            Loop
            Send Beginning_Of_Data   to (dblstbranch(self))
            Set Dynamic_Update_State of (dblstbranch(self)) to true
        
            Set Value Of frmEktypCode Item 0 to "r18_3DeliveryNew.rpt"
        
            Procedure_Return rVal
        
        End_Procedure
        
        Procedure Request_Cancel
            Close Tmplg
            Open Tmplg Mode DF_SHARE
            Forward Send Request_Cancel
        End_Procedure
        
        
        
        Object DeliveryWQLReport Is A CrystalReportsubsql
        
            Property Integer ReportReady Public False
            Property Integer piIsExcel  Public 0
            Property Integer piIsAscii  Public 0
        
            Set Report_View_id To (Parent(Current_object))
        
            Procedure OnStartPrinting
        
                Local string    sClientCode sCNCode  sItemCode  sCategoryCode sHmgioCode sstr sbranch scode skat sbr sEktypCode
                Local string    sClientName sCNDescr sItemDescr sCategoryDescr sHmgioDescr dHmniaApo dHmniaEos sname sSeira srep sEktypDescr
                Local string    endtypkinn FileName flysh1 flysh2 slabel shmgio stext
                Local Integer   Destination Selection SortOrder NumSortFields CurSortField repstatus FileType
                Local Integer   SortDirection wdate fepil starttypkinn iint icheck icount irec iSX
                date            apo eos
        
                move "" to sstr
                move "" to sBranch
                move "" to sText
        
                Get Select_Count of (dblstbranch(Self)) to iInt
        
                If (iInt>0) Begin
                    move '' to stext
                    move '' to sBranch
                    For iCount from 0 to (iInt-1)
                        Get array_value of (Selected_Items(dblstbranch(self))) Item iCount to iRec
                        Clear branch
                        Move iRec to branch.Recnum
                        Find Eq branch.Recnum
                        move (Trim(branch.branch_code)) to scode
                        move (Trim(branch.branch_name)) to sname
                        Move (Append(sbranch,(trim(sname)+' '))) to sbranch
                        Move (Append(stext  ,(trim(scode)+' '))) to stext
                    Loop
                End
                Move (Trim(sBranch)) to sBranch
                Move (Trim(sText  )) to sText
        
                get psModule                           to flysh1
                Get Value Of frmHmApo           Item 0 to dHmniaApo
                Get Value Of frmHmEos           Item 0 to dHmniaEos
                Get Value Of frmClientCode      Item 0 to sClientCode
                Get Value Of frmClientName      Item 0 to sClientName
                Get Value Of frmCNCode          Item 0 to sCNCode
                Get Value Of frmCNDescr         Item 0 to sCNDescr
                Get Value Of frmEktypCode       Item 0 to sEktypCode
                Get Value Of frmEktypDescr      Item 0 to sEktypDescr
                Get Value Of frmSeira           Item 0 to sSeira
        
                Set Formula "datefrom" to (Winql_date  (dHmniaApo))
                Set Formula "dateto"   to (Winql_date  (dHmniaEos))
                Set Formula "fhmgio"   to (Winql_String(sHmgioDescr))
                Set Formula "formula1" to (Winql_String(psModule(Self)))
                Set Formula "flabel"   to (Winql_String(sEktypDescr))
                Set Formula "fwhat"    to (Winql_number(piwhat(Self)))
                Set Formula "fkat"     to (Winql_String(sCategoryDescr))
                Set Formula "fbranch"  to (Winql_String(sBranch))
        
                If (Trim(sClientCode)>"") Begin
                    Set Formula "fClient"  to (Winql_String(sClientCode))
                    Set Formula "fClientN" to (Winql_String(sClientName))
                End
                Else Begin
                    Set Formula "fClient"  to (Winql_String("ANY"))
                    Set Formula "fClientN" to (Winql_String(""))
                End
        
                If (Trim(sCNCode)>"") Begin
                    Set Formula "fCN"      to (Winql_String(sCNCode))
                    Set Formula "fCNN"     to (Winql_String(sCNDescr))
                End
                Else Begin
                    Set Formula "fCN"      to (Winql_String("ANY"))
                    Set Formula "fCNN"     to (Winql_String(""))
                End
        
                Set Formula "fhmgio"   to (Winql_String(sHmgioDescr))
        
                 get epil to fepil
                 Set Formula "fepil" to (winql_number(fepil))
        
        //            If (piIsAscii(Self)=1) Begin
        ////                Set Output_Destination          to WQL_PRINT_TO_FILE
        ////                Set ExportFileName              to "c:\DeliveryExcelAscii.csv"
        ////                Set ExportFormatType            to WQL_CommaSeparatedType
        ////                Set FieldDelimiter              to ";"
        ////                Set StringDelimiter             to '"'
        ////                Set UseReportNumberFormat_State to True
        ////                Set UseReportDateFormat_State   To True
        //                Set Output_Device_Mode To WQL_PRINT_TO_WINDOW
        ////                Set Output_Destination          to WQL_PRINT_TO_WINDOW
        //                Set Output_Destination          to WQL_PRINT_TO_FILE
        //                Set ExportFileName              to "c:\DeliveryExcelA.CSV"
        //                Set ExportFormatType            to WQL_Xls5Type
        //            End
        //            Else If (piIsExcel(Self)=0) Begin
        //                Get DescAsc To SortDirection
        //                Get Output_Destination To Destination
        //                If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
        //                    Get WQLFileName      To FileName
        //                    Get WQLFileType      To FileType
        //                    Set ExportFileName   To FileName
        //                    Set ExportFormatType To FileType
        //                End
        //            End
        //            Else If (piIsExcel(Self)=1) Begin
        //                Set Output_Destination          to WQL_PRINT_TO_FILE
        //                Set ExportFileName              to "c:\DeliveryExcel.xls"
        //                Set ExportFormatType            to WQL_Xls5Type
        //            End
        
                Set Output_Device_Mode To WQL_PRINT_TO_WINDOW
                Set Output_Destination to WQL_PRINT_TO_WINDOW
        
                If Destination EQ WQL_PRINT_TO_WINDOW   Set ReportReady To False
                Else                                    Set ReportReady To True
        
            End_procedure // OnStartPrinting
        
            Procedure Runpmoves3Report
                String sRep sDescr
        
                Get psRep                           to sRep
                Get Value Of frmEktypDescr Item 0   to sDescr
        
                Move (UpperCase(sRep))              to sRep
                Move (UpperCase(sDescr))            to sDescr
        
                Set piIsExcel Of DeliveryWQLReport to 0
                Set piIsAscii Of DeliveryWQLReport to 0
        
                If (sDescr Contains "EXCEL") Begin
                    Set piIsExcel Of DeliveryWQLReport to 1
                End
                Else If (sDescr Contains "ASCII") Begin
                    Set piIsAscii Of DeliveryWQLReport to 1
                End
                Else Begin
                    Set piIsExcel Of DeliveryWQLReport to 0
                    Set piIsAscii Of DeliveryWQLReport to 0
                End
        
                Send Cursor_Wait To (Cursor_Control(Current_Object))
                Set Report_name  to (psrep(self))
                Send Run_report
                Send Cursor_Ready To (Cursor_Control(Current_Object))
        
        //            If (sDescr Contains "EXCEL") Begin
        //                Send DoStartDocument 'OPEN' "c:\DeliveryExcel.xls"
        //            End
        //            Else If (sDescr Contains "ASCII") Begin
        //                Send DoStartDocument 'OPEN' "c:\DeliveryExcelA.CSV"
        //            End
        
                Set piIsExcel Of DeliveryWQLReport to 0
                Set piIsAscii Of DeliveryWQLReport to 0
        
            End_procedure // RunCustReport
        
