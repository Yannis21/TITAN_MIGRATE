            //AB-StoreTopEnd

            Set DDO_Server to (Clients_DD(self))
            Set DDO_Server to (Constrct_DD(self))
            Set DDO_Server to (Salesmen_DD(self))
            Set DDO_Server to (Mhxanik_DD(self))

            //AB-StoreStart
                        
            Procedure Save_Main_File
                Move (piNomosID(Self))  to Cn.Cn_Nomos
                Move (piXoraID(Self))   to Cn.Cn_Xora
                If (cn.cn_end_date <> '') move 'F' to cn.cn_active
                else                      move 'A' to cn.cn_active
                Forward Send Save_Main_File
            End_Procedure
            
            
            Procedure Update
                Forward Send Update
            
                If (Operation_Origin=Self) begin
                    // Yannis 13/10/2017 Efyge o kodikas apo thn klash gia na mhn trexei
                    // stis periptoseis update ths klashs apo kleisimata h alles enhmeroseis
                    If ((piOldExportFlag(Self)=1) And (CN.ExportFlag=0)) Begin
                        Move "CANC" to CN.ExportStatus
                    End
                    Else Begin
                        Move 1      to CN.ExportFlag
                        Move "SEND" to CN.ExportStatus
                    End
                End
            End_Procedure
            
            
            
            //AB-StoreEnd

