                    Procedure Bring_Records_From_Client String asClient String asErgo
                        Clear TPClient
                        Move asClient to TPClient.Client_ID
                        Find Gt TPClient By Index.1 // Client_ID+...
                        While ((Found=1) And (TPClient.Client_ID=asClient))
                            Clear TPErga
                            Move asErgo                     to TPErga.CN_Code
                            Move TPClient.Baxiokat_Code     to TPErga.Baxiokat_Code
                            Move TPClient.Pct_Epi_Synolou   to TPErga.Pct_Epi_Synolou
                            Move TPClient.Paralabh_Meres    to TPErga.Paralabh_Meres
                            Move TPClient.Xarhtos_Meres     to TPErga.Xarhtos_Meres
                            Move TPClient.Plhroteo_Meres    to TPErga.Plhroteo_Meres
                            Move (Sysrecc.TPErga_Rec0+1)    to TPErga.TPErga_Rec0
                            Reread Sysrecc
                            Move (Sysrecc.TPErga_Rec0+1)    to Sysrecc.TPErga_Rec0
                            Saverecord Sysrecc
                            UnLock
                            Move (Append('',((Trim(SysCurXR.Branch_Site))+;
                                             (Trim(String(TPErga.TPErga_Rec0))))));
                                                            to TPErga.TPErga_Recr
                            Saverecord TPErga
                            Find Gt TPClient By Index.1 // Client_ID+...
                        Loop
                        Send Beginning_Of_Data to TPErga_ True
                    End_Procedure
                    
                    
                    Procedure OnClick
                        Integer iRecnum iErga rVal iVal
                        String sClient sErgo
                        Send Beginning_Of_Data to TPErga_ True
                        Get Field_Current_Value Of CN_DD Field Cn.Recnum                to iRecnum
                        Get Field_Current_Value Of CN_DD Field Cn.CN_Code               to sErgo
                        Get Field_Current_Value Of TPErga_DD Field TPErga.Recnum        to iErga
