                        end
                    end
                    if (scodegl contains '@2');
                        replace '@2' in scodegl with (trim(sbanka))
                    if (scodegl contains '@1');
                        replace "@1" in scodegl with (trim(syscurxr.branch_code))
                    Move (Check_log(scodegl)) to iCheck
