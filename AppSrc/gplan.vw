    Procedure InitSysmenu Integer aiState
        Integer hWnd hSmId bState
    
        If (aiState) Begin
            Get Window_Handle to hWnd
            If (hWnd <> 0) Begin
                Move (GetSystemMenu(hWnd,False)) to hSmId
                Move (DeleteMenu(hSmId,8,MF_BYPOSITION)) to bState
                Move (DeleteMenu(hSmId,7,MF_BYPOSITION)) to bState
            End
        End
    End_Procedure //InitSysmenu
    
    //***************** Predefined Procedures and Functions Section **************
    
    Procedure Activate_View Returns Integer
        Integer Rval liState  hWnd hSmId bState
    
        Forward Get MSG_Activate_View to Rval
        Get Modal_State of oGplan_View_Properties to liState
        Set Block_Mouse_State to liState
        Set Minimize_Icon to (1-liState)
        If (liState) On_Key kAction_Bar       Send None
        Send InitSysmenu liState
        Procedure_Return rVal
    End_Procedure // Activate_View
    
    Function InitProperties Returns Integer
        Integer liInvObjId
        Set Modal_State of oGplan_View_Properties to False
        Set Minimize_Icon to True
        Set Block_Mouse_State to False
        Get Invoking_Object_Id of oGplan_View_Properties to liInvObjId
        Set Invoking_Object_Id of oGplan_View_Properties to 0
        On_Key kAction_Bar       Send Default_key
        Function_Return liInvObjId
    End_Function // InitProperties
    
    Procedure Deactivating Returns Integer
        Integer rVal liInvObjId
    
        Forward Get MSG_Deactivating to rVal
        If (Not(rVal)) Begin
            Get InitProperties to liInvObjId
        End
        Procedure_Return rVal
    End_Procedure // Deactivating
    
    Procedure Request_Cancel
        Integer liInvObjId
    
        If (Block_Mouse_State(Self)) Begin
            Get InitProperties to liInvObjId
            Forward Send Request_Cancel
            If (liInvObjId) Set Current_Scope to liInvObjId
        End
        Else Forward Send Request_Cancel
    End_Procedure
    
    Procedure Page Integer PageMode
        Forward Send Page PageMode
        Send InitSysmenu (Modal_State(oGplan_View_Properties(self)))
    End_Procedure // Page
    
    Procedure Switch_Prior_View
        If (Block_Mouse_State(Self)=0) Forward Send Switch_Prior_View
    End_Procedure // Switch_Prior_View
    
    
    
    //AB-StoreEnd

CD_End_Object    // Gplan_View


