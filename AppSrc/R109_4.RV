        Object pmoves3WQLReport Is A CrystalReportsubsql
        
            Property Integer ReportReady Public False
            Set Report_View_id To (Parent(Current_object))
        
            Procedure OnStartPrinting
        
                Local string  Startnumbern sstr Endnumbern Startcustn Endcustn
                Local integer Starthmen Endhmen FileType
                Local string  starttypkinn endtypkinn Selection FileName flysh1 flysh2
                Local Integer Destination SortOrder NumSortFields CurSortField
                Local Integer SortDirection wdate fepil
                date apo eos
                move '' to sstr
                //*** Setup the selection formula
        
                Move "" To Selection
                Append Selection (If(Selection <> "", " and ", ""))"{wcmoves.company_code} = " '"' (Trim(syscurxr.company_code)) '"'
                Append Selection (If(Selection <> "", " and ", ""))"{wcmoves.branch_code} = " '"' (Trim(syscurxr.branch_code)) '"'
                Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.year_year} = " '' (syscurxr.year_year) ''
                Append Selection (If(Selection <> "", " and ", ""))"{wcmoves.cnt_id} <> " '"' (Trim(sstr)) '"'
        
                Get ReportSort To SortOrder
                //*** Set a selection formula for the report.
                    Get Startnumber To Startnumbern
                    Get Endnumber   To Endnumbern
                    Get Startcust To Startcustn
                    Get Endcust   To Endcustn
        
                    If (Trim(StartNumbern)) ne "" ;
                      Append Selection (If(Selection <> "", " and ", "")) "TrimRight({wcmoves.witemx_code}) >= " '"' (Trim(startnumbern)) '"'
                    If (Trim(EndNumbern)) ne "" ;
                      Append Selection (If(Selection <> "", " and ", "")) "TrimRight({wcmoves.witemx_code}) <= " '"' (Trim(Endnumbern)) '"'
                    If (Trim(Startcustn)) ne "" ;
                      Append Selection (If(Selection <> "", " and ", "")) "TrimRight({wcmoves.cnt_id}) >= " '"' (Trim(startcustn)) '"'
                    If (Trim(Endcustn)) ne "" ;
                      Append Selection (If(Selection <> "", " and ", "")) "TrimRight({wcmoves.cnt_id}) <= " '"' (Trim(Endcustn)) '"'
        
                 Get Starttypkin To Starttypkinn
                 Get Endtypkin   to Endtypkinn
                If (Trim(Starttypkinn)) ne "" ;
                      Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.wtypkin_code} >= " '"' (Trim(starttypkinn)) '"'
                If (Trim(Endtypkinn)) ne "" ;
                      Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.wtypkin_code} <= " '"' (Trim(Endtypkinn)) '"'
                 Get starthme To starthmen
                 Get Endhme   To Endhmen
                 If (trim(starthmen)) ne 0 ;
                      Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.wcmove_hmnia} >= " (winql_date(starthmen))
                 If (trim(Endhmen)) ne 0 ;
                      Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.wcmove_hmnia} <= " (winql_date(Endhmen))
                 Set Formula "datefrom" to (Winql_date(starthmen))
                 Set Formula "dateto" to (Winql_date(endhmen))
                 Set Formula "fwhat" to (Winql_number (piwhat(Self)))
                 move '01'to flysh1
                 Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.module_from} = " '"' (Trim(flysh1)) '"'
                 Set SelectionFormula To Selection
        
                 //*** Change the formula so the report reflects the selection. Remove
                //*** quotes from the formula.
                get epil to fepil
        
                Set Formula "fepil" to (winql_number(fepil))
        
        
                If Selection Eq "" ;
                    Set Formula "SelTxt" To ('"' + "NONE" + '"')
                Else ;
                    Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
        
              //*** Setup new sort order
                Get NSortFields To NumSortFields
                For CurSortField From 0 To (NumSortFields - 1)
                Send DeleteNthSortField 0
                Loop
                Get DescAsc To SortDirection
                set GroupField PE_GROUPHEADER to "{wcmoves.cnt_id}"
                Set GroupSortDirection PE_GROUPHEADER to SortDirection
                Send AddSortField "{wcmoves.cn_code}" SortDirection
                Send AddSortField "{wcmoves.witemx_code}" SortDirection
                Send AddSortField "{wcmoves.wcmove_hmnia}" SortDirection
                //*** Handle export to file
                Get Output_Destination To Destination
                If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                    Get WQLFileName To FileName
                    Get WQLFileType To FileType
        
                    Set ExportFileName   To FileName
                    Set ExportFormatType To FileType
                End
        
                If Destination EQ WQL_PRINT_TO_WINDOW ;
                    Set ReportReady To False
                Else ;
                    Set ReportReady To True
            End_procedure // OnStartPrinting
        
        
            //***
            //*** Procedure: RunCustReport
            //*** Purpose  : Run the customer report
            //***
        
            Procedure Runpmoves3Report
                //*** We will be busy, change the cursor
                integer iepil
                Send Cursor_Wait To (Cursor_Control(Current_Object))
                get outstatus to iepil
                If (iepil=1)  Set Report_name  to "r109_4h.rpt"
                Else          Set Report_name  To "r109_4a.rpt"
        
                //*** Run the report
                Send Run_report
        
                //*** We are ready, change the cursor
                Send Cursor_Ready To (Cursor_Control(Current_Object))
        
                //*** Report you are finsihed for those seetings that do not use the
                //*** screen
                If (ReportReady(Current_object)) ;
                    Send Info_box "WinQL report ready."
            End_procedure // RunCustReport
        
        End_object // CustWQLReport
        
        
