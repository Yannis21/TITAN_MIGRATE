        Procedure Activate_View Returns Integer
            String sStr sModule StrDisp
            Integer rVal iint icheck
            string scompany six
            set value of (f_hme_apo(groupsub4(current_object))) to syscurxr.year_xrhshfrom
            set value of (f_hme_eos(groupsub4(current_object))) to syscurxr.year_xrhshto
            Forward Get Msg_Activate_View   to rVal
            Send Delete_Data                to (Selected_Items(dblstBRANCH(self)))
            Send Find to (branch_dd(self)) First_Record 1
            Set Dynamic_Update_State of (dblstbranch(self)) to false
            While (Found=1)
                get field_current_value of branch_dd field branch.company_code to scompany
                get field_current_value of branch_dd field branch.branch_apoth to icheck
                if ((syscurxr.company_code=scompany) and (icheck=1));
                    Set Array_Value of (Selected_Items(dblstbranch(self))) Item iInt to branch.Recnum
                Add 1 to iInt
                Send Find to (branch_dd(self)) Next_Record 1
            Loop
            Send Beginning_Of_Data    to (dblstbranch(self))
            Set Dynamic_Update_State of (dblstbranch(self)) to true
            Procedure_Return rVal
        End_Procedure
        
        Object carsWQLReport Is A CrystalReportSubSQL
            Property Integer ReportReady Public False
            Set Report_View_id To (Parent(Current_object))
        
            Procedure OnStartPrinting
                date datef datet apo eos
                local string  startapon  sbranch sCompany setoima slabel six
                Local integer iwhat ikat
                Local String  Selection FileName Startnumbern EndNumbern stype
                Local Integer FileType Destination SortOrder NumSortFields CurSortField
                Local Integer SortDirection wdate sepil iepil
                Integer iint icheck icount irec
                string  stext scode sname srecr sbrecr Starthmen Endhmen
                local number nqty
        
        //        Showln ""
        //        Showln ""
        
                move sysfile.wapokat_etoima to setoima
        //        Showln "sEtoima=" (Trim(setoima))
        
                Move (Trim(SyscurXr.Company_Name)) to sCompany
        
                Move "" To Selection
                get value of ffortio item 0 to nqty
                get value of combof  item 0 to six
                left six to stype 1
        
                get startapo    to startapon
                get startnumber to startnumbern
                get endnumber   to endnumbern
                get psbranch    to sbranch
        
                Append Selection (If(Selection <> "", " and ", ""))"{pmoves.company_code} = " '"' (Trim(syscurxr.company_code)) '"'
        //        Showln "Comp=" syscurxr.company_code
        
                Get Select_Count of (dblstbranch(Self)) to iInt
        
                If (iInt>0) Begin
                    move '' to stext
                    For iCount from 0 to (iInt-1)
                        Get array_value of (Selected_Items(dblstbranch(self))) Item iCount to iRec
                        Clear branch
                        Move iRec to branch.Recnum
                        Find Eq branch.Recnum
                        move branch.branch_code to scode
                        move branch.branch_name to sname
               //         move branch.branch_recr to sbrecr
                        Move (Append(sbranch,(trim(sname)+' '))) to sbranch
                        Move (Append(stext,(trim(scode)+' '))) to stext
              //          Move (Append(srecr,(trim(sbrecr)+' '))) to srecr
                    Loop
                End
                Move (Trim(stext)) to stext
                if (Trim(stext)<>"") Append Selection (If(Selection <> "", " and ", "")) "InStr('" stext "',{pmoves.branch_code})<>0"
        //        Showln "Branch=" (Trim(sText))
        
                Append Selection (If(Selection <> "", " and ", "")) "{pmoves.year_year} = " '' (syscurxr.year_year) ''
        //        Showln "Year=" syscurxr.year_year
        
        
            //    if (startapon ne '');
                Append Selection (If(Selection <> "", " and ", ""))"{pmoves.ptyppar_code} = " '"' (Trim(startapon)) '"'
        //        Showln "Ptyppar=" (Trim(startapon))
        
                Append Selection (If(Selection <> "", " and ", ""))"{pmoves.wapokat_code} = " '"' (Trim(setoima)) '"'
        //        Showln "sEtoima=" (Trim(setoima))
        
                if (startnumbern ne '');
                    Append Selection (If(Selection <> "", " and ", ""))"{cars.car_id} >= " '"' (Trim(startnumbern)) '"'
        
                If (endnumbern ne "") ;
                   Append Selection (If(Selection <> "", " and ", "")) "{cars.car_id} <= " '"' (trim(endnumbern)) '"'
        
                if (trim(stype)<>'');
                   Append Selection (If(Selection <> "", " and ", ""))"{cars.ctype_id} = " '"' (Trim(stype)) '"'
        
                get starthme to starthmen
                Get Endhme   To Endhmen
        
                If (trim(starthmen)) ne "" ;
                   Append Selection (If(Selection <> "", " and ", "")) "{pmoves.partl_hmnia} >= " (winql_date(starthmen))
        //        Showln "Apo Hmnia=" (winql_date(starthmen))
        
        
                If (trim(Endhmen)) ne "" ;
                   Append Selection (If(Selection <> "", " and ", "")) "{pmoves.partl_hmnia} <= " (winql_date(Endhmen))
        //        Showln "Eos Hmnia=" (winql_date(endhmen))
        
             //   if (psrep(self)='r54_1.rpt');
             //       Append Selection (If(Selection <> "", " and ", "")) "InStr('" srecr "',{brzones.branch_recr})<>0"
        
        
                Set SelectionFormula To Selection
        
        //        Showln "Selection Formula=" (Trim(Selection))
        
        
        
                If Selection Eq "" ;
                    Set Formula "SelTxt" To ('"' + "NONE" + '"')
                Else ;
                    Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
                get starthme to apo
                get endhme   to eos
        
                if (apo='') move syscurxr.year_xrhshfrom to apo
                if (eos='') move syscurxr.year_xrhshto   to eos
                Set Formula "fCompany" to (Winql_String(sCompany))
                Set Formula "fbranch" to (Winql_String(sbranch))
                Set Formula "fapo" To (winql_date(apo))
                Set Formula "feos" To (winql_date(eos))
                get value of form2 to slabel
                Set Formula "flabel" To  (winql_string(slabel))
        
                Get NSortFields To NumSortFields
                For CurSortField From 0 To (NumSortFields - 1)
                Send DeleteNthSortField 0
                Loop
                get descasc to sortdirection
                IF (PSREP(SELF)<>'r54_20.rpt') begin
                    set GroupField PE_GROUPHEADER to "{cars.car_id}"
                    Set GroupSortDirection PE_GROUPHEADER to SortDirection
                End
                else begin
                    Set Formula "fepilkib" To (winql_number(nqty))
                    Set Formula "fix" To (winql_string(six))
                end
                Send AddSortField "{pmoves.partl_hmnia}" SortDirection
                Send AddSortField "{pmoves.partl_no}" SortDirection
        
                Get Output_Destination To Destination
                If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                    Get WQLFileName To FileName
                    Get WQLFileType To FileType
        
                    Set ExportFileName   To FileName
                    Set ExportFormatType To FileType
                End
                Else If Destination Eq WQL_PRINT_TO_PRINTER ;
                Send SelectAndSetPrinter
        
                If Destination EQ WQL_PRINT_TO_WINDOW ;
                    Set ReportReady To False
                Else ;
                    Set ReportReady To True
            End_procedure // OnStartPrinting
        
        
            Procedure RuncarsReport
                Send Cursor_Wait To (Cursor_Control(Current_Object))
                Set Report_name  To (psrep(self))
                Send Run_report
                Send Cursor_Ready To (Cursor_Control(Current_Object))
                If (ReportReady(Current_object)) ;
                    Send Info_box "WinQL report ready."
            End_procedure // RunCarsReport
        
        End_object
        
