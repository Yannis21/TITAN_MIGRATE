                Object Baxio_Baxio_Fbarticl is a dbFormSub

                    //AB-StoreTopStart
                    Procedure Find_Barticls
                        Integer liBarticlRec0 liWhichBarticl
                        Date ldHmnia
                        String liNo liBtypkin
                    
                        Move Barticl.Barticl_Rec0 to liBarticlRec0
                    
                        Get Field_Current_Value of Baxio_DD Field Baxio.Baxio_Fpar_In to liWhichBarticl
                        If (liWhichBarticl <> 0) Begin
                            Clear Barticl
                            Move liWhichBarticl to Barticl.Barticl_Rec0
                            Find eq Barticl by Index.2
                            [Found] Begin
                                Move Barticl.Barticl_Hmnia   to ldHmnia
                                Move Barticl.Barticl_No      to liNo
                                Move Barticl.Btypkin_Code    to liBtypkin
                            End
                        End
                        Set Value of dbfrmHmnia   Item 0 to ldHmnia
                        Set Value of dbfrmNo      Item 0 to liNo
                        Set Value of dbfrmGtypkin Item 0 to liBtypkin
                    
                        Move "" to ldHmnia
                        Move "" to liNo
                        Move "" to liBtypkin
                    
                        Get Field_Current_Value of Baxio_DD Field Baxio.Baxio_Fpar_Out to liWhichBarticl
                        If (liWhichBarticl <> 0) Begin
                            Clear Barticl
                            Move liWhichBarticl to Barticl.Barticl_Rec0
                            Find eq Barticl by Index.2
                            [Found] Begin
                                Move Barticl.Barticl_Hmnia   to ldHmnia
                                Move Barticl.Barticl_No      to liNo
                                Move Barticl.Btypkin_Code    to liBtypkin
                            End
                        End
                        Set Value of (dbfrmHmnia(dbGrpPlhrotea(Self)))   Item 0 to ldHmnia
                        Set Value of (dbfrmNo(dbGrpPlhrotea(Self)))      Item 0 to liNo
                        Set Value of (dbfrmGtypkin(dbGrpPlhrotea(Self))) Item 0 to liBtypkin
                    
                        Move "" to ldHmnia
                        Move "" to liNo
                        Move "" to liBtypkin
                    
                        Get Field_Current_Value of Baxio_DD Field Baxio.Baxio_Lpar to liWhichBarticl
                        If (liWhichBarticl <> 0) Begin
                            Clear Barticl
                            Move liWhichBarticl to Barticl.Barticl_Rec0
                            Find eq Barticl by Index.2
                            [Found] Begin
                                Move Barticl.Barticl_Hmnia   to ldHmnia
                                Move Barticl.Barticl_No      to liNo
                                Move Barticl.Btypkin_Code    to liBtypkin
                            End
                        End
                        Set Value of (dbfrmHmnia(dbGrpCurrentStat(Self)))   Item 0 to ldHmnia
                        Set Value of (dbfrmNo(dbGrpCurrentStat(Self)))      Item 0 to liNo
                        Set Value of (dbfrmGtypkin(dbGrpCurrentStat(Self))) Item 0 to liBtypkin
                    
                        Clear Barticl
                        Move liBarticlRec0 to Barticl.Barticl_Rec0
                        Find eq Barticl by Index.2
                    End_Procedure
                    
