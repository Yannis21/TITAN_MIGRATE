    Object tmpWQLReport Is A crystalreportSubSQL
    
        Property Integer ReportReady Public False
        Set Report_name  To "r26_2.rpt"
        Set Report_View_id To (Parent(Current_object))
    
        Procedure OnStartPrinting
            date datef datet apo eos
            local string startkathn startapon lscatypes Startantlian Endantlian
            Local integer Starthmen Endhmen FileType Destination SortOrder sepil
            Local String  Selection FileName StartantliaNumber EndantliaNumber
            Local Integer NumSortFields CurSortField SortDirection wdate
            local integer epilasc epilqty ino
    
            //*** Setup the selection formula
            Move "" To Selection
            Get StartNumber To StartantliaNumber
            Get EndNumber   To EndantliaNumber
            get startapo to startapon
            Get Endhme   To Endhmen
            get starthme to starthmen
            If Startapon ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "TrimRight({bttmp1.wax_recr}) = " '"' (trim(startapon)) '"'
    
            If StartantliaNumber ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "TrimRight({bttmp1.witemx_code}) >= " '"' (trim(startantlianumber)) '"'
    
            If EndantliaNumber ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "TrimRight({bttmp1.witemx_code}) <= " '"' (trim(Endantlianumber)) '"'
       //     If (trim(starthmen)) ne 0 ;
       //           Append Selection (If(Selection <> "", " and ", "")) "{bttmp1.wcmove_hmnia} >= " (winql_date(starthmen))
       //     If (trim(Endhmen)) ne 0 ;
       //           Append Selection (If(Selection <> "", " and ", "")) "{bttmp1.wcmove_hmnia} <= " (winql_date(Endhmen))
    
    
            Set SelectionFormula To Selection
    
            If Selection Eq "" ;
                Set Formula "SelTxt" To ('"' + "NONE" + '"')
            Else ;
                Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
            get  epil26 to sepil
            get starthme to apo
            get endhme   to eos
            Set Formula "fepil" to (winql_number(sepil))
            Set Formula "fapo" To (winql_date(apo))
            Set Formula "feos" To (winql_date(eos))
            Set Formula "fwhat" to (winql_number(piwhat(self)))
            Get NSortFields To NumSortFields
            For CurSortField From 0 To (NumSortFields - 1)
            Send DeleteNthSortField 0
            Loop
            get descasc to sortdirection
            set GroupField PE_GROUPHEADER to "{bttmp1.wax_recr}"
            Set GroupSortDirection PE_GROUPHEADER to SortDirection
            get piasc to epilasc
            get piti  to epilqty
            get fno  to ino
            set formula "fari" to ino
            if (epilasc = 0) begin
                set formula "fepilasc" to 1
            end
            else begin
                set formula "fepilasc" to 0
            end
            if (epilqty = 0) begin
                set formula "fepilqty" to 1
            end
            else begin
                set formula "fepilqty" to 0
            end
            if (epilasc = 0) begin
                if (epilqty = 0) begin
                   Send AddSortField "{bttmp1.wcmove_qty_exa}" PE_SF_descending SortDirection
                end
                else begin
                   Send AddSortField "{bttmp1.wcmove_p0}" PE_SF_descending SortDirection
                end
            end
            else begin
                if (epilqty = 0) begin
                   Send AddSortField "{bttmp1.wcmove_qty_exa}" PE_SF_ascending SortDirection
                end
                else begin
                   Send AddSortField "{bttmp1.wcmove_p0}" PE_SF_ascending SortDirection
                end
            end
            //*** Handle export to file
            Get Output_Destination To Destination
            If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                Get WQLFileName To FileName
                Get WQLFileType To FileType
    
                Set ExportFileName   To FileName
                Set ExportFormatType To FileType
            End
            Else If Destination Eq WQL_PRINT_TO_PRINTER ;
            Send SelectAndSetPrinter
    
            //*** Setup the report if ready property
            If Destination EQ WQL_PRINT_TO_WINDOW ;
                Set ReportReady To False
            Else ;
                Set ReportReady To True
        End_procedure // OnStartPrinting
    
    
        Procedure RuntmpReport
            //*** We will be busy, change the cursor
            Send Cursor_Wait To (Cursor_Control(Current_Object))
    
            //*** Run the report
            Send Run_report
    
            //*** We are ready, change the cursor
            Send Cursor_Ready To (Cursor_Control(Current_Object))
    
            //*** Report you are finsihed for those seetings that do not use the
            //*** screen
            If (ReportReady(Current_object)) ;
                Send Info_box "WinQL report ready."
        End_procedure // RunCustReport
    
    End_object // CustWQLReport
    
