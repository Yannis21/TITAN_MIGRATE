    Object cdtlWQLReport Is A CrystalReportSubsql
    
    
    
        Property Integer ReportReady Public False
            set Report_name    To "r94_3.rpt"
            Set Report_View_id To (Parent(Current_object))
    
       // Local Integer Count
       // local integer CurSortDirection
    
    
         Procedure OnStartPrinting
            local integer sepil Starthmen Endhmen startqty SortDirection CurSortField
            Local String  Startmovess Endmovess Selection FileName StartmovesNumber
            Local string  EndmovesNumber
            Local Integer FileType Destination SortOrder NumSortFields
            move 0    to startqty
            //*** Setup the selection formula
            Move "" To Selection
            Get ReportSort To SortOrder
    
                       //*** Set a selection formula for the report.
            Get Startmovedd To Startmovess
            Get Endmovedd   To Endmovess
    
            If (Trim(Startmovess)) Ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{clorderd.item_code} >= " '"' (Trim(Startmovess)) '"'
            If (Trim(Endmovess)) Ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{clorderd.item_code} <= " '"' (Trim(Endmovess)) '"'
    
            Get StartNumber To StartmovesNumber
            Get EndNumber   To EndmovesNumber
            If (trim(StartmovesNumber)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{clorderd.clients_code} >= " '"' (Trim(startmovesnumber)) '"'
            If (trim(EndmovesNumber)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{clorderd.clients_code} <= " '"' (Trim(Endmovesnumber)) '"'
    
            Append Selection (If(Selection <> "", " and ", "")) "{clorderd.module} = " '"' (psmodule(self)) '"'
            Append Selection (If(Selection <> "", " and ", "")) "{clorderd.company_code} = " '"' (Trim(syscurxr.company_code)) '"'
            Append Selection (If(Selection <> "", " and ", "")) "{clorderd.branch_code} = " '"' (Trim(syscurxr.branch_code)) '"'
            Append Selection (If(Selection <> "", " and ", "")) "{clorderd.year_year} = " '' (syscurxr.year_year) ''
    
    
            get epil to sepil
    
            If sepil eq 1 begin
                 //   Append Selection (If(Selection <> "", " and ", "")) "{clorderd.ypol_qty}  <= " '' (0)''
                    Append Selection (If(Selection <> "", " and ", "")) "{clorderd.rial_qty}  >  " '' (0)''
            end
            If sepil eq 2 ;
                    Append Selection (If(Selection <> "", " and ", "")) "{clorderd.ypol_qty}  > " '' (0)''
            If sepil eq 3 ;
                    Append Selection (If(Selection <> "", " and ", "")) "{clorderd.akyrh_qty}  > " '' (0)''
    
    
            Get Starthme To Starthmen
            Get Endhme   To Endhmen
    
            If (trim(Starthmen)) ne 0 ;
                Append Selection (If(Selection <> "", " and ", "")) "{clorderd.date} >= " (winql_date(starthmen))
    
            If (trim(Endhmen)) ne 0 ;
                Append Selection (If(Selection <> "", " and ", "")) "{clorderd.date} <= " (winql_date(Endhmen))
    
            set formula 'fmodule' to (winql_string(psmodule(self)))
            Set SelectionFormula To Selection
    
             //*** Change the formula so the report reflects the selection. Remove
            //*** quotes from the formula.
             Set Formula "fepil" To ('"' + (Replaces('"', sepil, "")) + '"')
            If Selection Eq "" ;
                Set Formula "SelTxt" To ('"' + "NONE" + '"')
            Else ;
                Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
    
          //*** Setup new sort order
            Get NSortFields To NumSortFields
            For CurSortField From 0 To (NumSortFields - 1)
            Send DeleteNthSortField 0
            Loop
            Get DescAsc To SortDirection
    
            set GroupField PE_GROUPHEADER to "{clorderd.clients_code}"
            Set GroupSortDirection PE_GROUPHEADER to SortDirection
            Send AddSortField "{clorderd.aa_par}" SortDirection
            Send AddSortField "{clorderd.item_code}" SortDirection
    
            //*** Handle export to file
            Get Output_Destination To Destination
            If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                Get WQLFileName To FileName
                Get WQLFileType To FileType
    
                Set ExportFileName   To FileName
                Set ExportFormatType To FileType
            End
            //Else If Destination Eq WQL_PRINT_TO_PRINTER ;
            //    Send SelectAndSetPrinter
    
            //*** Setup the report if ready property
            If Destination EQ WQL_PRINT_TO_WINDOW ;
                Set ReportReady To False
            Else ;
                Set ReportReady To True
        End_procedure // OnStartPrinting
    
    
        //***
        //*** Procedure: RunCustReport
        //*** Purpose  : Run the customer report
        //***
    
        Procedure RuncdtlReport
            //*** We will be busy, change the cursor
            Send Cursor_Wait To (Cursor_Control(Current_Object))
    
            //*** Run the report
            Send Run_report
    
            //*** We are ready, change the cursor
            Send Cursor_Ready To (Cursor_Control(Current_Object))
    
            //*** Report you are finsihed for those seetings that do not use the
            //*** screen
            If (ReportReady(Current_object)) ;
                Send Info_box "WinQL report ready."
        End_procedure // RunCustReport
    
    End_object // CustWQLReport
    
    
