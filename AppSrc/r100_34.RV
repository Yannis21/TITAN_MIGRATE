    Object tmpcWQLReport Is A crystalreportSubSQL
    
        Property Integer ReportReady Public False
    
        Set Report_View_id To (Parent(Current_object))
    
        Procedure OnStartPrinting
            date datef datet apo eos
            Local string  Startclient Endclient Selection FileName smodule
            Local integer Starthmen Endhmen FileType Destination SortOrder
            Local Integer NumSortFields CurSortField SortDirection wdate count
            local integer sepil epilasc epilqty ino iperioxh cursortdirection
            local string spresp  ssls flysh1 sbranch sstr slabel sls presp area
            Integer iint icheck icount irec
            string  stext scode sname
            get psModule  to sModule
    
            //*** Setup the selection formula
            Move '' to sStr
            Move "" To Selection
            get value of (sls_apo(groupsub5(self))) to ssls
            get value of (presp_apo(groupsub5(self))) to spresp
            get value of (f_perioxh(groupsub5(self))) to iperioxh
    
            Get StartNumber To Startclient
            Get EndNumber   To Endclient
            GET starthme to starthmen
            Get Endhme   To Endhmen
            Append Selection (If(Selection <> "", " and ", ""))"{wcmoves.company_code} = " '"' (Trim(syscurxr.company_code)) '"'
            Get Select_Count of (dblstbranch(Self)) to iInt
            If (iInt>0) Begin
                move '' to stext
                For iCount from 0 to (iInt-1)
                    Get array_value of (Selected_Items(dblstbranch(self))) Item iCount to iRec
                    Clear branch
                    Move iRec to branch.Recnum
                    Find Eq branch.Recnum
                    move branch.branch_code to scode
                    move branch.branch_name to sname
                    Move (Append(sbranch,(trim(sname)+' '))) to sbranch
                    Move (Append(stext,(trim(scode)+' '))) to stext
                Loop
            End
    
            Append Selection (If(Selection <> "", " and ", "")) "InStr('" stext "',{wcmoves.branch_code})<>0"
            Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.year_year} = " '' (syscurxr.year_year) ''
            Append Selection (If(Selection <> "", " and ", ""))"{wcmoves.client_id} <> " '"' (Trim(sstr)) '"'
    
            Set Formula "fbranch" to (Winql_String(sbranch))
            get  psModule to flysh1
            Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.module_from} = " '"' (Trim(flysh1)) '"'
            If (trim(Startclient)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.client_id} >= " '"' (trim(startclient)) '"'
            If (trim(Endclient)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.client_id} <= " '"' (trim(Endclient)) '"'
    
            If (trim(Endhmen)) ne 0 ;
                  Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.wcmove_hmnia} <= " (winql_date(Endhmen))
            If (trim(starthmen)) ne 0 ;
                  Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.wcmove_hmnia} >= " (winql_date(starthmen))
            If (trim(Ssls)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.sls_id} = " '"' (trim(ssls)) '"'
            If (trim(Spresp)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.presp_id} = " '"' (trim(spresp)) '"'
            If (trim(iperioxh)) ne 0 ;
               Append Selection (If(Selection <> "", " and ", "")) "{cn.cn_area} = " '' (iperioxh) ''
    
            Set SelectionFormula To Selection
    
            If Selection Eq "" ;
                Set Formula "SelTxt" To ('"' + "NONE" + '"')
            Else ;
                Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
            get  epil100 to sepil
            get starthme to apo
            get endhme   to eos
            Set Formula "fepil" to (winql_number(sepil))
            Set Formula "fapo"  To (winql_date(apo))
            Set Formula "feos"  To (winql_date(eos))
            Set Formula "fwhat" to (winql_number(piwhat(self)))
            get value of form2 to slabel
            Set Formula "flabel" to (winql_string(slabel))
            get value of (formsub6(groupsub5(self))) to sls
            get value of (formsub7(groupsub5(self))) to presp
            get value of (formsub8(groupsub5(self))) to area
            Set Formula "fsls" to (winql_string(sls))
            Set Formula "fpresp" to (winql_string(presp))
            Set Formula "farea" to (winql_string(area))
            Get NSortFields To NumSortFields
            For CurSortField From 0 To (NumSortFields - 1)
            Send DeleteNthSortField 0
            Loop
    
            get descasc to sortdirection
            get piasc to epilasc
            get piti  to epilqty
    
            set formula "fmodule" to (winql_string(smodule))
            if (epilasc = 0) begin
                set formula "fepilasc" to 1
            end
            else begin
                set formula "fepilasc" to 0
            end
            if (epilqty = 0) begin
                set formula "fepilqty" to 1
            end
            else begin
                set formula "fepilqty" to 0
            end
            move 0 to ino
            if (psrep(self)='r100_34.rpt');
                set GroupField PE_GROUPheader to "{wcmoves.branch_code}"
            if (psrep(self)='r100_34s.rpt');
                set GroupField PE_GROUPheader to "{wcmoves.sls_id}"
            if (psrep(self)='r100_34p.rpt');
                move 1 to ino
            if (psrep(self)='r100_34pa.rpt');
                move 1 to ino
            if (psrep(self)='r100_34g.rpt');
                move 1 to ino
            if (psrep(self)='r100_34a.rpt');
                set GroupField PE_GROUPheader to "{cn.cn_area}"
            if (psrep(self)='r100_34c.rpt');
                move 1 to ino
    
            if (ino<>1);
                set GroupField (PE_GROUPheader+1) to "{wcmoves.cn_code}"
          if (psrep(self)<>'r100_34g.rpt') begin
            if (epilasc = 0) begin
                if (epilqty = 0) begin
                   if (smodule = "01") begin
                       Send AddGROUPSortField "Sum ({@fexa}, {wcmoves.cn_code})" PE_SF_descending
                       Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                   else begin
                       Send AddGROUPSortField "Sum ({@feisa}, {wcmoves.cn_code})" PE_SF_descending
                       Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                end
                else begin
                    if (smodule = "01") begin
                       Send AddGROUPSortField "Sum ({@cmovesp}, {wcmoves.cn_code})" PE_SF_descending
                       Set GroupSortDirection PE_GROUPheader to SortDirection
                    end
                    else begin
                       Send AddGROUPSortField "Sum ({@cmovesp}, {wcmoves.cn_code})" PE_SF_descending
                       Set GroupSortDirection PE_GROUPheader to SortDirection
                    end
                end
            end
            else begin
                if (epilqty = 0) begin
                   if (smodule = "01") begin
                      Send AddGROUPSortField "Sum ({@fexa}, {wcmoves.cn_code})" PE_SF_ascending
                      Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                   else begin
                      Send AddGROUPSortField "Sum ({@feisa}, {wcmoves.cn_code})" PE_SF_ascending
                      Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                end
                else begin
                   if (smodule = "01") begin
                      Send AddGROUPSortField "Sum ({@cmovesp}, {wcmoves.cn_code})" PE_SF_ascending
                      Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                   else begin
                      Send AddGROUPSortField "Sum ({@cmovesp}, {wcmoves.cn_code})" PE_SF_ascending
                      Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                end
            end
          end
          else begin
            if (epilasc = 0) begin
                if (epilqty = 0) begin
                   if (smodule = "01") begin
                       Send AddGROUPSortField "Sum ({@fexa}, {wcmoves.presp_id})" PE_SF_descending
                       Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                   else begin
                       Send AddGROUPSortField "Sum ({@feisa}, {wcmoves.presp_id})" PE_SF_descending
                       Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                end
                else begin
                    if (smodule = "01") begin
                       Send AddGROUPSortField "Sum ({@cmovesp}, {wcmoves.presp_id})" PE_SF_descending
                       Set GroupSortDirection PE_GROUPheader to SortDirection
                    end
                    else begin
                       Send AddGROUPSortField "Sum ({@cmovesp}, {wcmoves.presp_id})" PE_SF_descending
                       Set GroupSortDirection PE_GROUPheader to SortDirection
                    end
                end
            end
            else begin
                if (epilqty = 0) begin
                   if (smodule = "01") begin
                      Send AddGROUPSortField "Sum ({@fexa}, {wcmoves.presp_id})" PE_SF_ascending
                      Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                   else begin
                      Send AddGROUPSortField "Sum ({@feisa}, {wcmoves.presp_id})" PE_SF_ascending
                      Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                end
                else begin
                   if (smodule = "01") begin
                      Send AddGROUPSortField "Sum ({@cmovesp}, {wcmoves.presp_id})" PE_SF_ascending
                      Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                   else begin
                      Send AddGROUPSortField "Sum ({@cmovesp}, {wcmoves.presp_id})" PE_SF_ascending
                      Set GroupSortDirection PE_GROUPheader to SortDirection
                   end
                end
            end
          end
    
            //*** Handle export to file
            Get Output_Destination To Destination
            If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                Get WQLFileName To FileName
                Get WQLFileType To FileType
    
                Set ExportFileName   To FileName
                Set ExportFormatType To FileType
            End
            Else If Destination Eq WQL_PRINT_TO_PRINTER ;
            Send SelectAndSetPrinter
    
            //*** Setup the report if ready property
            If Destination EQ WQL_PRINT_TO_WINDOW ;
                Set ReportReady To False
            Else ;
                Set ReportReady To True
        End_procedure // OnStartPrinting
    
    
