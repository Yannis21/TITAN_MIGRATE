        Function Allow_Child_Entry Returns Integer
            Integer iObj iSrvr SaveMsg
            String  sCN sClient
            Get Server to iSrvr
            If (Should_Save(Self)=0 AND Current_record(iSrvr)=0) Begin
                Error 300 'Éú§ ¨ß·®Æú† Ü£ú®„©†¶ Éú¢´Â¶.'
                Function_Return 1
            End
        
            Get Field_Current_Value Of Clients_DD   Field Clients.Client_ID to sClient
            Get Field_Current_Value Of CN_DD        Field CN.CN_Code        to sCN
            If (Trim(sClient)='' Or Trim(sCN)='') Begin
                If (sClient='') Send Info_Box 'Éú§ ‚Æú´ú úß†¢‚•ú† ßú¢·´û'   'è¢û®¶≠¶®Âò'
                Else            Send Info_Box 'Éú§ ‚Æú´ú úß†¢‚•ú† ‚®ö¶'     'è¢û®¶≠¶®Âò'
                Function_Return 1
            End
        
            If (Current_record(iSrvr) OR Confirmation("åò òß¶üû°úÁ©‡ ´¶ §‚¶ õú¢´Â¶ ;")=0) Begin
                Get Verify_Save_MSG to SaveMsg             // the old confirmation
                Set Verify_Save_MSG to GET_No_Confirmation // disable confirmation
                Send Request_Save_no_Clear                 // save it
                Set Verify_Save_MSG to SaveMsg             // restore old confirm
            End
            Function_Return (Should_Save(Self))
        
        End_Function  // Allow_Child_Entry
        
        
        Procedure Activate_View Returns Integer
            Integer rVal
            String  sID
            Forward Get Msg_Activate_View to rVal
            Get psID            to sID
            Move (Trim(sID))    to sID
            If (sID<>'') Begin
                Clear DorderH
                Move Syscurxr.Company_Code  to DorderH.Company_Code
                Move sID                    to DorderH.DorderH_ID
                Find Eq DorderH by Index.1
                If (Found) Send Request_Find to DorderH_DD Eq DorderH.File_Number 1
            End
            Procedure_Return rVal
        End_Procedure
        
