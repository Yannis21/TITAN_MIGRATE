        Object ButtonSub1 is a ButtonSub
            Set Label to "ButtonSub1"
            Set Bitmap to "Rigaft.bmp"
            Set Size to 18 23
            Set Location to 259 245
            Set Focus_Mode to Pointer_Only
            Set Form_TypeFace Item 0 to "MS Sans Serif"

            //AB-StoreStart
                                                                                                                                                                                                                                                                    
            Procedure OnClick
                String  sBranch sCompany sID sRecr
                String  sSite sCN sBranchRecr sLocked
                Integer iCount isFound iErr iObjID
                Move 0 to iErr
                Move 0 to iCount
            
                Get Field_Current_Value Of Clients_DD Field Clients.Client_Locked to sLocked
                If (sLocked='1') Begin
                    Send Stop_Box 'é ßú¢·´û™ úÂ§ò† °¢ú†õ‡£‚§¶™.\nÉú§ úß†´®‚ßú´ò† òß¶©´¶¢„ ©ú ß®¶ö®ò££ò´†©£Ê' 'Error 5000'
                    Procedure_Return
                End
            
                Get Field_Current_Value Of Branch_DD2 Field Branch.Branch_Code   to sBranch
                Get Field_Current_Value Of Branch_DD2 Field Branch.Beto_Site     to sSite
                Get Field_Current_Value Of Branch_DD2 Field Branch.Branch_Recr   to sBranchRecr
                If (Trim(sBranch)='') Begin
                    Send Stop_Box 'Éú§ ‚Æú´ú úß†¢‚•ú† ´¶£‚ò' 'ë≠·¢£ò'
                    Procedure_Return
                End
                Get Field_Current_Value Of DorderH_DD Field DorderH.Company_Code    to sCompany
                Get Field_Current_Value Of DorderH_DD Field DorderH.DorderH_ID      to sID
                Get Field_Current_Value Of DorderH_DD Field DorderH.DorderH_Recr    to sRecr
                Get Field_Current_Value Of DorderH_DD Field DorderH.CN_Code         to sCN
            
            
                Send ChangeAllFileModes DF_Filemode_ReadOnly
                Set_Attribute DF_File_Mode Of HmerProg.File_Number  to DF_Filemode_Default
                Set_Attribute DF_File_Mode Of DorderD.File_Number   to DF_Filemode_Default
            
                Indicate Err False
                BEGIN_TRANSACTION
                Move Error_Object_ID to iObjID
            
                Clear DorderD
                Move sRecr to DorderD.DorderH_Recr
                Find Gt DorderD by Index.1
                While ((Found=1) And (DorderD.DorderH_Recr=sRecr))
                    If (DorderD.Ypol_Posothta>0) Begin
                        Add 1 to iCount
                        Clear HmerProg
                        Move sBranchRecr            to HmerProg.Branch_Recr
                        Move DorderD.Recnum         to HmerProg.DorderD_Recnum
                        Find Eq HmerProg by index.1
                        Move (Found)                to isFound
                        If (isFound) Reread HmerProg
                        Move sID                    to HmerProg.DorderH_ID
                        Move DorderD.Hmnia_Parad    to HmerProg.Hmnia
                        Move DorderD.Ora_Parad      to HmerProg.StartTime
                        Move sBranch                to HmerProg.Site
                        Move sSite                  to HmerProg.SiteW
                        Add DorderD.Ypol_Posothta   to HmerProg.Qty
                        Move sCompany               to HmerProg.Company_Code
                        Move DorderD.Item_Code      to HmerProg.Item_Code
                        Move DorderD.Pump_ID        to HmerProg.Pump_ID
                        Move DorderD.Pumpm          to HmerProg.Pumpm
                        Move sCN                    to HmerProg.CN_Code
                        Move 1                      to HmerProg.Selected
                        Saverecord HmerProg
                        If (Err) Begin
                            Move 1 to iErr
                            Abort_Transaction
                            Goto vw051_Abort1
                        End
                        Reread DorderD
                        Add DorderD.Ypol_Posothta   to DorderD.Qty_Se_Progr
                        Move (DorderD.Posothta-DorderD.Qty_Se_Progr) ;
                                                    to DorderD.Ypol_Posothta
                        If (DorderD.Ypol_Posothta<0) Error 6001 'vw051: ìßÊ¢¶†ß¶ ß¶©Ê´û´ò™ < 0'
                        Saverecord DorderD
                        Unlock
                        If (isFound) Unlock
                        If (Err) Begin
                            Move 2 to iErr
                            Abort_Transaction
                            Goto vw051_Abort1
                        End
                    End
                    Find Gt DorderD by Index.1
                Loop
                END_TRANSACTION
                vw051_Abort1:
                Unlock
                Send ChangeAllFileModes DF_Filemode_Default
                Move iObjID to Error_Object_ID
                If (iErr) Begin
                        If (iErr=1) Send Stop_Box 'è®Êô¢û£ò °ò´· ´û§ úöö®ò≠„ HmerProg'      'ë≠·¢£ò 1001'
                   Else If (iErr=2) Send Stop_Box 'è®Êô¢û£ò °ò´· ´û§ ú§û£‚®‡©û Dorderd'     'ë≠·¢£ò 1002'
                End
                Else Begin
                    If (iCount>0) Send Info_Box 'Ü õ†òõ†°ò©Âò ¶¢¶°¢û®Èüû°ú' 'è¢û®¶≠¶®Âò'
                    Else          Send Stop_Box 'Éú§ ô®‚üû°ò§ úÂõû £ú ß¶©Ê´û´ò õ†òü‚©†£û\nö†ò §ò òß¶©´ò¢úÂ ©ú ß®¶ö®ò££ò´†©£Ê.' 'è¢û®¶≠¶®Âò'
                End
                Send Beginning_Of_Data to DorderD_ True
                Send Beginning_Of_Data to HmerProg_ True
            End_Procedure // OnClick
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
