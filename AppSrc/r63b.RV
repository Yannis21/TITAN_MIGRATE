    Object custWQLReport Is A CrystalReportSubSQL
    
        Property Integer ReportReady Public False
    
        Set Report_View_id To (Parent(Current_object))
        Set Report_name  To "r6_3b.rpt"
      //  Set Report_name  To "report1.rpt"
        Procedure OnStartPrinting
            date datef datet  apo eos
            local string  lscatypes Startantlian Endantlian sstr sbranch flysh1
            Local integer Starthmen Endhmen FileType Destination SortOrder sepil
            Local String  Selection module FileName StartantliaNumber EndantliaNumber
            Local Integer NumSortFields CurSortField SortDirection wdate iok iwhat iti
            local number  mhden
            //*** Setup the selection formula
            Move "" To Selection
            get  epil0 to sepil
            get piwhat to iwhat
            get pishow to iti
            get psbranch to sbranch
            get psModule to flysh1
            move 0 to mhden
            Get ReportSort To SortOrder
            Reread SysCurXr
            Unlock
            Append Selection (If(Selection <> "", " and ", ""))"{cmoves.company_code} = " '"' (Trim(syscurxr.company_code)) '"'
            If sbranch ne "" ;
                Append Selection (If(Selection <> "", " and ", ""))"{cmoves.branch_code} = " '"' (Trim(sbranch)) '"'
            Append Selection (If(Selection <> "", " and ", "")) "{cmoves.year_year} = " '' (syscurxr.year_year) ''
    //        if (iti=1) Append Selection (If(Selection <> "", " and ", "")) "{bttmp1.wcmove_x0} > {bttmp1.wcmove_p0}"
    //        if (iti=2) Append Selection (If(Selection <> "", " and ", "")) "{bttmp1.wcmove_p0} > {bttmp1.wcmove_x0}"
    //        if (iti=3) Append Selection (If(Selection <> "", " and ", "")) "({bttmp1.wcmove_p0} <> 0 or {bttmp1.wcmove_x0} <> 0)"
    //        if (iti=3) Append Selection (If(Selection <> "", " and ", "")) "{bttmp1.wcmove_p0} = {bttmp1.wcmove_x0}"
    //        if (iti=4) Append Selection (If(Selection <> "", " and ", "")) "{bttmp1.wcmove_x0} = 0"
    //        if (iti=4) Append Selection (If(Selection <> "", " and ", "")) "{bttmp1.wcmove_p0} = 0"
    //        if (iti=5) Append Selection (If(Selection <> "", " and ", "")) "({bttmp1.wcmove_p0} <> 0 or {bttmp1.wcmove_x0} <> 0)"
            Append Selection (If(Selection <> "", " and ", "")) "{cmoves.module_from} = " '"' (trim(flysh1)) '"'
            Get StartNumber To StartantliaNumber
            Get EndNumber   To EndantliaNumber
            get starthme    to starthmen
            Get Endhme      To Endhmen
            if (starthmen=0) move syscurxr.year_xrhshfrom to starthmen
            if (endhmen=0)   move syscurxr.year_xrhshto   to endhmen
            Append Selection (If(Selection <> "", " and ", "")) "{cmoves.cmoves_hmnia} >= " (winql_date(starthmen))
            Append Selection (If(Selection <> "", " and ", "")) "{cmoves.cmoves_hmnia} <= "  (winql_date(Endhmen))
    
            If (trim(StartantliaNumber)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{clients.client_id} >= " '"' (trim(startantlianumber)) '"'
    
            If (trim(EndantliaNumber)) ne "" ;
               Append Selection (If(Selection <> "", " and ", "")) "{clients.client_id} <= " '"' (trim(Endantlianumber)) '"'
    
            Set Formula "formula1" to (Winql_String(psModule(Self)))
            get  epil0 to sepil
            move starthmen to apo
            move endhmen   to eos
            Set Formula "fepil" to (winql_number(sepil))  ////////To ('"' + (Replaces('"', fepil, "")) + '"')
            Set Formula "fapo" To (winql_date(apo))
            Set Formula "feos" To (winql_date(eos))
            Set Formula "fti" to (winql_number(iti))
            set formula 'fwhat' to (winql_number(iwhat))
            clear branch
            move syscurxr.company_code to branch.company_code
            move sbranch               to branch.branch_code
            find eq branch by index.1
            Set Formula "fbranch" to (Winql_String(branch.branch_name))
    
            Set SelectionFormula To Selection
    
             //*** Change the formula so the report reflects the selection. Remove
            //*** quotes from the formula.
            If Selection Eq "" ;
                Set Formula "SelTxt" To ('"' + "NONE" + '"')
            Else ;
                Set Formula "SelTxt" To ('"' + (Replaces('"', Selection, "")) + '"')
    
    
          //*** Setup new sort order
            Get NSortFields To NumSortFields
            For CurSortField From 0 To (NumSortFields - 1)
            Send DeleteNthSortField 0
            Loop
            get descasc to sortdirection
            If SortOrder Eq cln_name;
               set GroupField PE_GROUPHEADER to "{clients.client_name}"
            else;
               set GroupField PE_GROUPHEADER to "{clients.client_id}"
            //*** Handle export to file
            Get Output_Destination To Destination
            If ((Destination Eq WQL_PRINT_TO_FILE) Or (Destination Eq WQL_PRINT_TO_MAPI)) Begin
                Get WQLFileName To FileName
                Get WQLFileType To FileType
    
                Set ExportFileName   To FileName
                Set ExportFormatType To FileType
            End
            Else If Destination Eq WQL_PRINT_TO_PRINTER ;
            Send SelectAndSetPrinter
    
            //*** Setup the report if ready property
            If Destination EQ WQL_PRINT_TO_WINDOW ;
                Set ReportReady To False
            Else ;
                Set ReportReady To True
        End_procedure // OnStartPrinting
    
    
        //***
        //*** Procedure: RunCustReport
        //*** Purpose  : Run the customer report
        //***
    
        Procedure RuncustReport
            //*** We will be busy, change the cursor
            Send Cursor_Wait To (Cursor_Control(Current_Object))
            Send Run_report
    
            //*** We are ready, change the cursor
            Send Cursor_Ready To (Cursor_Control(Current_Object))
    
            //*** Report you are finsihed for those seetings that do not use the
            //*** screen
            If (ReportReady(Current_object)) ;
                Send Info_box "WinQL report ready."
        End_procedure // RunCustReport
    
    End_object // CustWQLReport
    
    
