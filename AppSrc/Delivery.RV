        Procedure Runpmoves3Report
            String sRep sDescr
            Get psRep                           to sRep
            Get Value Of frmEktypDescr Item 0   to sDescr
            Move (UpperCase(sRep))              to sRep
            Move (UpperCase(sDescr))            to sDescr
    
            Set piIsExcel Of pmoves3WQLReport to 0
            Set piIsAscii Of pmoves3WQLReport to 0
    
            If (sDescr Contains "EXCEL") Begin
                Set piIsExcel Of pmoves3WQLReport to 1
            End
            Else If (sDescr Contains "ASCII") Begin
                Set piIsAscii Of pmoves3WQLReport to 1
            End
            Else Begin
                Set piIsExcel Of pmoves3WQLReport to 0
                Set piIsAscii Of pmoves3WQLReport to 0
            End
    
            Send Cursor_Wait To (Cursor_Control(Current_Object))
            Set Report_name  to (psrep(self))
            Send Run_report
            Send Cursor_Ready To (Cursor_Control(Current_Object))
    
    //            If (sDescr Contains "EXCEL") Begin
    //                Send DoStartDocument 'OPEN' "c:\DeliveryExcel.xls"
    //            End
    //            Else If (sDescr Contains "ASCII") Begin
    //                Send DoStartDocument 'OPEN' "c:\DeliveryExcelA.CSV"
    //            End
    
            Set piIsExcel Of pmoves3WQLReport to 0
            Set piIsAscii Of pmoves3WQLReport to 0
    
        End_procedure // RunCustReport
    
