        Object cmovesWQLReport Is A CrystalReportSubSQL
        
            Property Integer ReportReady Public False
        
        // Set Output_Destination          to WQL_PRINT_TO_WINDOW
           Set Output_Destination          to WQL_PRINT_TO_FILE
           Set ExportFileName              to "c:\apot.txt"
           Set UseReportNumberFormat_State to True
           Set ExportFormatType            to WQL_CommaSeparatedType
           Set FieldDelimiter              to ","
           Set StringDelimiter             to '"'
           Set UseReportDateFormat_State   To True
           Set Report_View_id To (Parent(Current_object))
        
            Procedure OnStartPrinting
        
                local string  lscatypes Startnumbern Endnumbern Startnamen Endnamen
                Local integer Starthmen Endhmen StartantliaNumber starttypkinn
                Local string  Selection FileName slabel spol
                Local integer EndantliaNumber FileType Destination SortOrder;
                              NumSortFields CurSortField SortDirection wdate
                local integer fepil iOK   iwhat iepil ichk iarea iypokat
                string flysh1 sStr flysh2  module sbranch blank SPER
                date   apo eos
                Integer iint icheck icount irec
                string  stext scode sname
                number nmin nmax
                move '' to blank
                MOVE '03' TO SPER
                get pspol   to spol
                get piarea  to iarea
                set iokey to 0
                //*** Setup the selection formula
                reread syscurxr
                unlock
                get value of (fmin(self)) to nmin
                get value of (fmax(self)) to nmax
                get checked_state of (checkbox1(self))  to ichk
                get checked_state of (fypokat(self))    to iypokat
                Append Selection (If(Selection <> "", " and ", ""))"{wcmoves.company_code} = " '"' (Trim(syscurxr.company_code)) '"'
                Get Select_Count of (dblstbranch(Self)) to iInt
                If (iInt>0) Begin
                    move '' to stext
                    For iCount from 0 to (iInt-1)
                        Get array_value of (Selected_Items(dblstbranch(self))) Item iCount to iRec
                        Clear branch
                        Move iRec to branch.Recnum
                        Find Eq branch.Recnum
                        move branch.branch_code to scode
                        move branch.branch_name to sname
                        Move (Append(sbranch,(trim(sname)+' '))) to sbranch
                        Move (Append(stext,(trim(scode)+' '))) to stext
                    Loop
                End
        //        Append Selection (If(Selection <> "", " and ", "")) "InStr('" stext "',{WCMOVES.BRANCH_CODE})<>0"
                Append Selection (If(Selection <> "", " and ", ""))"{wcmoves.client_id}<> " '"' (Trim(blank)) '"'
                Get StartNumber To StartNumbern
                Get EndNumber   To Endnumbern
                If (Trim(StartNumbern)) ne "" ;
                    Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.client_id} >= " '"' (Trim(startnumbern)) '"'
                If (Trim(EndNumbern)) ne "" ;
                   Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.client_id} <= " '"' (Trim(Endnumbern)) '"'
                If (Trim(spol)) ne "" ;
                    Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.sls_id} = " '"' (Trim(spol)) '"'
                if (iarea<>0);
                    Append Selection (If(Selection <> "", " and ", "")) "{cn.cn_area} = " '' (iarea) ''
        
        
                Get Starthme To starthmen
                Get Endhme   To Endhmen
                 If (trim(starthmen)) ne 0 ;
                      Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.wcmove_hmnia} >= " (winql_date(starthmen))
                 If (trim(Endhmen)) ne 0 ;
                      Append Selection (If(Selection <> "", " and ", "")) "{wcmoves.wcmove_hmnia} <= " (winql_date(Endhmen))
                 Append Selection (If(Selection <> "", " and ", ""))"{wcmoves.wapokat_code} = " '"' (Trim(sysfile.wapokat_etoima)) '"'
                 Get Starttypkin To Starttypkinn
                 Move '' to sStr
                 clear hmwtyp
                 move starttypkinn to hmwtyp.hmgiow_code
                 find gt hmwtyp by index.1
                 while  ((found) and (hmwtyp.hmgiow_code=starttypkinn))
                    Move (Append(sStr,(trim(hmwtyp.wtypkin_code)+' '))) to sStr
                    find gt hmwtyp by index.1
                 loop
                 Move (Trim(sStr)) to sStr
                 Append Selection (If(Selection <> "", " and ", "")) "InStr('" sStr "',{wcmoves.wtypkin_code})<>0"
        
                 Set Formula "fwhat" to (Winql_number(piwhat(Self)))
        
                 Set SelectionFormula To Selection
        
                 //*** Change the formula so the report reflects the selection. Remove
                //*** quotes from the formula.
                get starthme to apo
                get endhme   to eos
