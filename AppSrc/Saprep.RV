         move 0 to inum1
         Move (psPath(Self)+Trim(stomeas)+'FI2'+Trim(sEtos)+Trim(sMhnas);
                +Trim(sHmera)+'.txt')   to sFileName
         Direct_Input Channel 9 sFileName
